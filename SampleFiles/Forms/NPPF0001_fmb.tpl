      <Trigger Name="WHEN-NEW-FORM-INSTANCE" TriggerText="PR_POPULATE_LISTS;&amp;#10;&amp;#10;-- call procedure to set form titles&amp;#10;   WHEN_NEW_FORM_INSTANCE;&amp;#10;&amp;#10;if &#x9;:parameter.npp_id is not null&amp;#10;then &#x9;set_item_property('control.profile_button',displayed,property_false);&amp;#10;     &#x9;execute_query;&amp;#10;end if;&amp;#10;&amp;#10;pr_update_user;&amp;#10;&amp;#10;  IF   USER = 'BEARD_D'&amp;#10;  THEN &amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_TAXABLE_IND',ENABLED,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_TAXABLE_IND',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  ELSE &amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_TAXABLE_IND',ENABLED,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_TAXABLE_IND',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;  &amp;#10;&amp;#10;  :CONTROL.NBT_UNSUITABLE_IND := PK_CCO.FN_ACTION_ALLOWED('NPPF0001','UNSUITABLE',USER);&amp;#10;&amp;#10;-- IF   USER IN ('CCO','KAPTERIAN_M','COX_N','MAUGHAN_N','WEHRLE_C')&amp;#10;&#x9;IF   :CONTROL.NBT_UNSUITABLE_IND = 'Y'&amp;#10;  THEN &amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_EXM_UNSUITABLE_IND',ENABLED,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_EXM_UNSUITABLE_IND',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  ELSE &amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_EXM_UNSUITABLE_IND',ENABLED,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NPP.NPP_EXM_UNSUITABLE_IND',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;  &amp;#10;&amp;#10;  :CONTROL.NBT_BANK_IND := PK_CCO.FN_ACTION_ALLOWED('NPPF0001','BANK',USER);&amp;#10;&amp;#10;  IF&#x9;:CONTROL.NBT_BANK_IND = 'Y'&amp;#10;  THEN&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_ACCOUNT_NO',UPDATEABLE,PROPERTY_TRUE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_ACCOUNT_NAME',UPDATEABLE,PROPERTY_TRUE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_SORT_CODE',UPDATEABLE,PROPERTY_TRUE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_NAME',UPDATEABLE,PROPERTY_TRUE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_BRANCH_NAME',UPDATEABLE,PROPERTY_TRUE);&amp;#10;  ELSE&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_ACCOUNT_NO',UPDATEABLE,PROPERTY_FALSE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_ACCOUNT_NAME',UPDATEABLE,PROPERTY_FALSE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_SORT_CODE',UPDATEABLE,PROPERTY_FALSE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_NAME',UPDATEABLE,PROPERTY_FALSE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('NPP.NPP_BANK_BRANCH_NAME',UPDATEABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;&amp;#10;--wi1358&amp;#10;--&#x9;SET_ITEM_PROPERTY('CONTROL.HMRC_Q_BUTTON',DISPLAYED,PROPERTY_FALSE);&amp;#10;&#x9;SET_ITEM_PROPERTY('CONTROL.HMRC_Q_BUTTON',DISPLAYED,PROPERTY_TRUE);&amp;#10;&#x9;&#x9;SET_ITEM_PROPERTY('CONTROL3.BT_RECALCULATE_PAYGROUP',DISPLAYED,PROPERTY_TRUE);"/>
      <Trigger Name="WHEN-WINDOW-CLOSED" TriggerText="if :system.event_window = 'WINDOW0' then&amp;#10;--------------------------&amp;#10;&#x9;--EC2115 &amp;#10;&#x9;IF :NADD.NADD_ATYP_CODE = 'H' AND &amp;#10;&#x9;&#x9; :NADD.NADD_CIA2_ID IS NOT NULL &amp;#10;  THEN&amp;#10;&#x9;   DECLARE&amp;#10;&#x9;       CURSOR c_hmrc_int IS&amp;#10;&#x9;       SELECT NADD_CIA2_ID&amp;#10;&#x9;       FROM  NPP_ADDRESSES&amp;#10;&#x9;       WHERE NADD_NPP_ID = :NPP.NPP_ID&amp;#10;&#x9;       AND   NADD_ATYP_CODE = 'H';&amp;#10;&#x9;  &amp;#10;         v_old_nadd_cia2_id npp_addresses.nadd_cia2_id%type := null;&amp;#10;&amp;#10;     BEGIN&amp;#10;         v_old_nadd_cia2_id := null;&amp;#10;&amp;#10;&#x9;       OPEN c_hmrc_int;&amp;#10;&#x9;       FETCH c_hmrc_int INTO v_old_nadd_cia2_id;&amp;#10;&#x9;       CLOSE c_hmrc_int;&amp;#10;&#x9;&amp;#10;&#x9;&#x9;    &amp;#10;&#x9;&#x9;     IF&#x9;  :NADD.NADD_CIA2_ID &lt;> 'GB' &amp;#10;&#x9;&#x9;     THEN&amp;#10;&#x9;&#x9;        IF  :NPP.NPP_HMRC_INT_Q3 IS NULL AND&amp;#10;&#x9;&#x9;            :NPP.NPP_HMRC_INT_Q4 IS NULL AND&amp;#10;&#x9;&#x9;            :NPP.NPP_HMRC_INT_Q5 IS NULL AND &amp;#10;&#x9;              :NPP.NPP_HMRC_INT_Q6 IS NULL AND&amp;#10;&#x9;              :NPP.NPP_HMRC_INT_Q7 IS NULL AND&amp;#10;&#x9;&#x9;            :NPP.NPP_HMRC_INT_Q8 IS NULL&amp;#10;&#x9;&#x9;        THEN&amp;#10;&#x9;&#x9;           SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9;           'ERROR...answers to ''INTERNATIONAL HMRC QUESTIONS'' required...'||&amp;#10;                      '...Use ''HMRC Qs'' button.');&amp;#10;&amp;#10;               IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;           THEN &amp;#10;                  RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;           END IF;&amp;#10;&#x9;         END IF; &amp;#10;&#x9;&#x9;       &amp;#10;&#x9;&#x9;       IF :NADD.NADD_CIA2_ID &lt;> v_old_nadd_cia2_id &amp;#10;&#x9;&#x9;       THEN&amp;#10;&#x9;&#x9;  &#x9;     PR_RECALCULATE_PAYGROUP;&#x9;&amp;#10;&#x9;&#x9;         COMMIT_FORM;&amp;#10;&#x9;&#x9;       END IF;&amp;#10;&#x9;&#x9;    &amp;#10;&#x9;&#x9;     ELSIF :NADD.NADD_CIA2_ID = 'GB'&amp;#10;&#x9;&#x9;     THEN  &#x9;&amp;#10;&#x9;&#x9;       IF  :NADD.NADD_CIA2_ID &lt;> v_old_nadd_cia2_id &amp;#10;&#x9;&#x9;       THEN&amp;#10;&#x9;&#x9;         :NPP.NPP_HMRC_INT_Q3 := NULL;&amp;#10;&#x9;           :NPP.NPP_HMRC_INT_Q4 := NULL;&amp;#10;&#x9;&#x9;         :NPP.NPP_HMRC_INT_Q5 := NULL; &amp;#10;&#x9;           :NPP.NPP_HMRC_INT_Q6 := NULL;&amp;#10;&#x9;           :NPP.NPP_HMRC_INT_Q7 := NULL;&amp;#10;&#x9;&#x9;         :NPP.NPP_HMRC_INT_Q8 := NULL;&amp;#10;&#x9;&#x9;         &amp;#10;&#x9;&#x9;  &#x9;     PR_RECALCULATE_PAYGROUP;&#x9;&amp;#10;&#x9;&#x9;         COMMIT_FORM;&amp;#10;&#x9;&#x9;       END IF; &amp;#10;&#x9;&#x9;     END IF;&amp;#10;&#x9;&#x9; END;&amp;#10;  END IF;  --EC2115&amp;#10;--------------------------&amp;#10;    exit_form;&amp;#10;end if;&amp;#10;&amp;#10;IF :SYSTEM.EVENT_WINDOW = 'HMRC_WINDOW'&amp;#10;THEN&amp;#10;&amp;#10;-- EC4982 Added the below  IF-THEN-ELSIF condition for the new Paygroup recalculation logic &amp;#10;&#x9;&amp;#10;&#x9;IF :NPP.NPP_HMRC_INT_Q6 = 'Y' AND&amp;#10;    &#x9;:NPP.NPP_HMRC_INT_Q7 &lt;> 'GBP'&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9;&#x9;'WARNING !! Appropriate HMRC Qs response should be entered for Paygroup re-calculation ...'||&amp;#10;&#x9;   &#x9;'...Please change accordingly .');&amp;#10;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&#x9;&amp;#10;&#x9;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;&#x9;&#x9;&#x9;THEN &amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;ELSIF :NPP.NPP_NPAY_ID = 1 AND :NADD.NADD_CIA2_ID &lt;> 'GB'&amp;#10;  THEN&amp;#10;&#x9;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9;&#x9;'ERROR...Pay group cannot be 1 for Non ''GB'' country code...'||&amp;#10;&#x9;&#x9;&#x9;  '...Enter approprite flag for the ''HMRC Qs'' .');&amp;#10;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;&#x9;&#x9;&#x9;THEN &amp;#10;&#x9;&#x9;&#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;&#x9;END IF;&amp;#10;  ELSE&amp;#10;    GO_BLOCK('NPP');&amp;#10;  END IF;&amp;#10;  &amp;#10;&amp;#10;END IF;&amp;#10;&amp;#10;if &#x9;:system.event_window &lt;> 'BANK_WINDOW'&amp;#10;then&#x9;&amp;#10;&#x9;:system.message_level := '5';&amp;#10;&#x9;previous_record;&amp;#10;&#x9;:system.message_level := '0';&amp;#10;end if;&amp;#10;&amp;#10;go_block('npp');&amp;#10;&amp;#10;if :system.event_window IN ('NRCE_WINDOW','NTAP_WINDOW','NRCO_WINDOW',&amp;#10;   'NRWA_WINDOW','NRRE_WINDOW1','NRRE_WINDOW2','NRBU_WINDOW',&amp;#10;   'NRTE_WINDOW','NRGS_WINDOW','NRGU_WINDOW','NREG_WINDOW','NAWA_WINDOW',&amp;#10;   'NRAV_WINDOW','NATR_WINDOW')  &amp;#10;then&amp;#10;    go_block('NROL');&amp;#10;end if;&amp;#10;&amp;#10;if :system.event_window = 'BANK_WINDOW'&amp;#10;then go_field('NPP.NPP_SURNAME');&amp;#10;end if;&amp;#10;&amp;#10;&amp;#10;hide_window(:system.event_window);"/>
