         <Trigger Name="KEY-DELREC" TriggerText="IF&#x9;:NPP.NPP_ID IS NULL&amp;#10;THEN&#x9;&amp;#10;&#x9;DELETE_RECORD;&amp;#10;ELSE&amp;#10;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;                   'NPPS CANNOT BE DELETED');&amp;#10;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;&#x9;THEN NULL;&amp;#10;&#x9;END IF;&amp;#10;END IF;"/>
         <Trigger Name="ON-CHECK-DELETE-MASTER" TriggerText="--&amp;#10;-- Begin default relation declare section&amp;#10;--&amp;#10;DECLARE&amp;#10;  Dummy_Define VARCHAR2(1);&amp;#10;  --&amp;#10;  -- Begin NQUA detail declare section&amp;#10;  --&amp;#10;  CURSOR NQUA_cur IS      &amp;#10;    SELECT 1 FROM NPP_QUALIFICATIONS     &amp;#10;    WHERE NQUA_NPP_ID = :NPP.NPP_ID;&amp;#10;  --&amp;#10;  -- End NQUA detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NLAN detail declare section&amp;#10;  --&amp;#10;  CURSOR NLAN_cur IS      &amp;#10;    SELECT 1 FROM NPP_LANGUAGES     &amp;#10;    WHERE NLAN_NPP_ID = :NPP.NPP_ID;&amp;#10;  --&amp;#10;  -- End NLAN detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NADD detail declare section&amp;#10;  --&amp;#10;  CURSOR NADD_cur IS      &amp;#10;    SELECT 1 FROM NPP_ADDRESSES     &amp;#10;    WHERE NADD_NPP_ID = :NPP.NPP_ID;&amp;#10;  --&amp;#10;  -- End NADD detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NASU detail declare section&amp;#10;  --&amp;#10;  CURSOR NASU_cur IS      &amp;#10;    SELECT 1 FROM NPP_ACADEMIC_SUBJECTS     &amp;#10;    WHERE NASU_NPP_ID = :NPP.NPP_ID;&amp;#10;  --&amp;#10;  -- End NASU detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NVSE detail declare section&amp;#10;  --&amp;#10;  CURSOR NVSE_cur IS      &amp;#10;    SELECT 1 FROM NPP_VOCATIONAL_SECTORS     &amp;#10;    WHERE NVSE_NPP_ID = :NPP.NPP_ID;&amp;#10;  --&amp;#10;  -- End NVSE detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NAWA detail declare section&amp;#10;  --&amp;#10;  CURSOR NAWA_cur IS      &amp;#10;    SELECT 1 FROM NPP_AWARDS     &amp;#10;    WHERE NAWA_NPP_ID = :NPP.NPP_ID;&amp;#10;  --&amp;#10;  -- End NAWA detail declare section&amp;#10;  --&amp;#10;--&amp;#10;-- End default relation declare section&amp;#10;--&amp;#10;--&amp;#10;-- Begin default relation program section&amp;#10;--&amp;#10;BEGIN&amp;#10;  --&amp;#10;  -- Begin NQUA detail program section&amp;#10;  --&amp;#10;  OPEN NQUA_cur;     &amp;#10;  FETCH NQUA_cur INTO Dummy_Define;     &amp;#10;  IF ( NQUA_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NQUA_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NQUA_cur;&amp;#10;  --&amp;#10;  -- End NQUA detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NLAN detail program section&amp;#10;  --&amp;#10;  OPEN NLAN_cur;     &amp;#10;  FETCH NLAN_cur INTO Dummy_Define;     &amp;#10;  IF ( NLAN_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NLAN_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NLAN_cur;&amp;#10;  --&amp;#10;  -- End NLAN detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NADD detail program section&amp;#10;  --&amp;#10;  OPEN NADD_cur;     &amp;#10;  FETCH NADD_cur INTO Dummy_Define;     &amp;#10;  IF ( NADD_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NADD_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NADD_cur;&amp;#10;  --&amp;#10;  -- End NADD detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NASU detail program section&amp;#10;  --&amp;#10;  OPEN NASU_cur;     &amp;#10;  FETCH NASU_cur INTO Dummy_Define;     &amp;#10;  IF ( NASU_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NASU_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NASU_cur;&amp;#10;  --&amp;#10;  -- End NASU detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NVSE detail program section&amp;#10;  --&amp;#10;  OPEN NVSE_cur;     &amp;#10;  FETCH NVSE_cur INTO Dummy_Define;     &amp;#10;  IF ( NVSE_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NVSE_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NVSE_cur;&amp;#10;  --&amp;#10;  -- End NVSE detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NAWA detail program section&amp;#10;  --&amp;#10;  OPEN NAWA_cur;     &amp;#10;  FETCH NAWA_cur INTO Dummy_Define;     &amp;#10;  IF ( NAWA_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NAWA_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NAWA_cur;&amp;#10;  --&amp;#10;  -- End NAWA detail program section&amp;#10;  --&amp;#10;END;&amp;#10;--&amp;#10;-- End default relation program section&amp;#10;--&amp;#10;"/>
         <Trigger Name="ON-POPULATE-DETAILS" TriggerText="--&amp;#10;-- Begin default relation declare section&amp;#10;--&amp;#10;DECLARE&amp;#10;  recstat     VARCHAR2(20) := :System.record_status;   &amp;#10;  startitm    VARCHAR2(61) := :System.cursor_item;   &amp;#10;  rel_id      Relation;&amp;#10;--&amp;#10;-- End default relation declare section&amp;#10;--&amp;#10;--&amp;#10;-- Begin default relation program section&amp;#10;--&amp;#10;BEGIN&amp;#10;  IF ( recstat = 'NEW' or recstat = 'INSERT' ) THEN   &amp;#10;    RETURN;&amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- Begin NQUA detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NQUA');   &amp;#10;    Query_Master_Details(rel_id, 'NQUA');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NQUA detail program section&amp;#10;  --&amp;#10;&amp;#10;  --&amp;#10;  -- Begin NLAN detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NLAN');   &amp;#10;    Query_Master_Details(rel_id, 'NLAN');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NLAN detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NADD detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NADD');   &amp;#10;    Query_Master_Details(rel_id, 'NADD');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NADD detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NASU detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NASU');   &amp;#10;    Query_Master_Details(rel_id, 'NASU');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NASU detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NVSE detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NVSE');   &amp;#10;    Query_Master_Details(rel_id, 'NVSE');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NVSE detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NROL detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NROL');   &amp;#10;    Query_Master_Details(rel_id, 'NROL');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NROL detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NAWA detail program section&amp;#10;  --&amp;#10;  IF ( (:NPP.NPP_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NPP.NPP_NAWA');   &amp;#10;    Query_Master_Details(rel_id, 'NAWA');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NAWA detail program section&amp;#10;  --&amp;#10;&amp;#10;  IF ( :System.cursor_item &lt;> startitm ) THEN     &amp;#10;     Go_Item(startitm);     &amp;#10;     Check_Package_Failure;     &amp;#10;  END IF;&amp;#10;&amp;#10;  PR_FORMAT_NPP_BUTTONS;&amp;#10;&amp;#10;END;&amp;#10;--&amp;#10;-- End default relation program section&amp;#10;--&amp;#10;"/>
         <Trigger Name="PRE-INSERT" TriggerText="IF&#x9;:NPP.NPP_GRADUATE_EXM_IND = 'Y'&amp;#10;THEN&amp;#10;&amp;#10;&#x9;DECLARE N NUMBER;&amp;#10;&amp;#10;  BEGIN&amp;#10;&amp;#10;&#x9;&#x9;N := SHOW_ALERT('GRADUATE');&amp;#10;&amp;#10;&#x9;&#x9;IF    N = ALERT_BUTTON2&amp;#10; &#x9;&#x9;THEN  RAISE FORM_TRIGGER_FAILURE;&amp;#10; &#x9;&#x9;END IF;&amp;#10;&amp;#10;  END;&amp;#10;&amp;#10;END IF;&amp;#10;&amp;#10;PR_VALIDATE_DUPE;&amp;#10;&amp;#10;DECLARE &amp;#10;&#x9;P_NPP_ID &#x9;&#x9;&#x9; VARCHAR2(100);&amp;#10;&amp;#10;&#x9;N NUMBER;&amp;#10;&amp;#10;    &#x9;CURSOR C1 IS &#x9;SELECT &#x9;TO_CHAR(NPP_ID) NPP_ID&amp;#10;&#x9;&#x9; &#x9;FROM   &#x9;NPPS&amp;#10;&#x9;&#x9; &#x9;WHERE  &#x9;NPP_SURNAME&#x9;  = :NPP.NPP_SURNAME&amp;#10;&#x9;&#x9; &#x9;AND&#x9;NPP_DATE_OF_BIRTH IS NOT NULL&amp;#10;&#x9;&#x9;&#x9;AND&#x9;:NPP.NPP_DATE_OF_BIRTH IS NOT NULL&amp;#10;&#x9;&#x9; &#x9;AND    &#x9;NPP_DATE_OF_BIRTH = :NPP.NPP_DATE_OF_BIRTH;&amp;#10;&amp;#10;BEGIN&amp;#10;&amp;#10;&#x9;P_NPP_ID := NULL;&amp;#10;&amp;#10;&#x9;FOR R1 IN C1 LOOP&amp;#10;&amp;#10;&#x9;&#x9;P_NPP_ID := P_NPP_ID||R1.NPP_ID||' ';&amp;#10;&amp;#10;&#x9;END LOOP;&amp;#10; &amp;#10; &#x9;IF &#x9;P_NPP_ID IS NOT NULL&amp;#10; &#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;SET_ALERT_PROPERTY('DUPLICATE_NPP2',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9; 'WARNING...ONE OR MORE AA''S EXIST WITH MATCHING SURNAME AND DATE OF BIRTH...'||P_NPP_ID||&amp;#10;                 '...DO YOU WISH TO CONTINUE ?');&amp;#10;&amp;#10;&#x9;&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&amp;#10;&#x9;&#x9;N := SHOW_ALERT('DUPLICATE_NPP2');&amp;#10;&amp;#10;&#x9;&#x9;IF    N = ALERT_BUTTON2&amp;#10; &#x9;&#x9;THEN  RAISE FORM_TRIGGER_FAILURE;&amp;#10; &#x9;&#x9;END IF;&amp;#10;&amp;#10; &#x9;END IF;&amp;#10;&amp;#10;END;&amp;#10;&amp;#10;PR_FORMAT_NAME;&amp;#10;PR_VALIDATE_NPP_STATUS;&amp;#10;PR_VALIDATE_GENDER_TITLE;&amp;#10;:NPP.NPP_INSERT_DATE := SYSDATE;&amp;#10;:NPP.NPP_INSERT_USER := USER;&amp;#10;&amp;#10;DECLARE&amp;#10;&amp;#10;&#x9;&#x9;P_GET_ID_MESS  VARCHAR2(100);&amp;#10;&#x9;&amp;#10;&#x9;BEGIN&amp;#10;&amp;#10;&#x9;IF&#x9;:NPP.NPP_GRADUATE_EXM_IND = 'Y'&amp;#10;&#x9;THEN&amp;#10;&#x9;-- New 20/02/09 -- jpd&amp;#10;&amp;#10;  pk_npp_id.pr_get_npp_id('Y', :NPP.npp_id, P_GET_ID_MESS);&amp;#10;&amp;#10;-- below commented out by jpd - 20/02/09&amp;#10;--&#x9;SELECT NVL(MAX(NPP_ID),319999)+1&amp;#10;--&#x9;INTO   :NPP.NPP_ID&amp;#10;--  FROM   NPPS&amp;#10; -- WHERE  NPP_ID BETWEEN 320000 AND 329999;&amp;#10;&amp;#10;&#x9;INSERT INTO NPP_ROLES&amp;#10;&#x9;(NROL_ID,                     &amp;#10;   NROL_NPP_ID,                 &amp;#10;   NROL_NRTY_ID,                &amp;#10;   NROL_START_DATE,             &amp;#10;   NROL_EMPLOYMENT_STATUS,      &amp;#10;   NROL_NOTES,                  &amp;#10;   NROL_INSERT_DATE,            &amp;#10;   NROL_INSERT_USER)&amp;#10;   SELECT&#x9;NROL_SEQ.NEXTVAL,&amp;#10;   &#x9;&#x9;&#x9;&#x9;:NPP.NPP_ID,&amp;#10;   &#x9;&#x9;&#x9;&#x9;237,&amp;#10;   &#x9;&#x9;&#x9;&#x9;:NPP.NPP_START_DATE,&amp;#10;   &#x9;&#x9;&#x9;&#x9;'CU',&amp;#10;   &#x9;&#x9;&#x9;&#x9;'AUTOMATICALLY CREATED',&amp;#10;   &#x9;&#x9;&#x9;&#x9;SYSDATE,&amp;#10;   &#x9;&#x9;&#x9;&#x9;USER&amp;#10;   &#x9;FROM&#x9;DUAL;          &amp;#10;&amp;#10;&#x9;ELSE&amp;#10;&#x9;&amp;#10;--&#x9;SELECT NPP_SEQ.NEXTVAL&amp;#10;--&#x9;INTO   :NPP.NPP_ID&amp;#10;--  FROM   DUAL;&amp;#10;  &amp;#10;  -- New 20/02/09 -- jpd&amp;#10;&amp;#10;  pk_npp_id.pr_get_npp_id('N', :NPP.npp_id, P_GET_ID_MESS);&amp;#10;  &amp;#10;--&#x9;SELECT  MIN(NPP_ID)+1&amp;#10;--&#x9;INTO&#x9;&#x9;:NPP.NPP_ID&amp;#10;--&#x9;FROM    NPPS NPP&amp;#10;--&#x9;WHERE   NPP_ID     BETWEEN 100000 AND 499999&amp;#10;--&#x9;AND     NPP_ID NOT BETWEEN 300000 AND 399999&amp;#10;--&#x9;AND     NPP_ID NOT BETWEEN 890000 AND 899999&amp;#10;--&#x9;AND     NPP_ID NOT BETWEEN 989999 AND 999999&amp;#10;--&#x9;AND NOT EXISTS (SELECT  NULL&amp;#10;--  &#x9;              FROM    NPPS&amp;#10;--    &#x9;            WHERE   NPP_ID = NPP.NPP_ID+1)&amp;#10;--&#x9;AND NOT EXISTS (SELECT  NULL&amp;#10;--  &#x9;              FROM    NPP_LOAD_XREF_DATA&amp;#10;--    &#x9;            WHERE   NLXD_ID = NPP.NPP_ID+1);&amp;#10;  &amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;END;&amp;#10;&#x9;&amp;#10;--EC2115&#x9;&amp;#10;PR_RECALCULATE_PAYGROUP;"/>
         <Trigger Name="PRE-UPDATE" TriggerText="PR_VALIDATE_NPP_STATUS;&amp;#10;PR_VALIDATE_GENDER_TITLE;&amp;#10;PR_FORMAT_NAME;&amp;#10;&amp;#10;IF&#x9;:CONTROL.NBT_UNSUITABLE_AA IN ('1','2')&amp;#10;THEN&amp;#10;&amp;#10;&#x9;BEGIN&amp;#10;&#x9;&amp;#10;&#x9;&#x9;UPDATE&#x9;NPP_ROLES&amp;#10;&#x9;&#x9;SET&#x9;&#x9;&#x9;NROL_EMPLOYMENT_STATUS &#x9;= 'EX',&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;NROL_END_DATE&#x9;&#x9;&#x9;&#x9;&#x9; &#x9;=&#x9;SYSDATE&amp;#10;&#x9;&#x9;WHERE&#x9;&#x9;NROL_NPP_ID&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;= :NPP.NPP_ID&amp;#10;&#x9;&#x9;AND&#x9;&#x9; (NROL_END_DATE&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;IS NULL&amp;#10;&#x9;&#x9;OR&#x9;&#x9;&#x9;NROL_EMPLOYMENT_STATUS&#x9;&lt;> 'EX');&amp;#10;&#x9;&amp;#10;&#x9;END;&amp;#10;&amp;#10;END IF;&amp;#10;&amp;#10;IF&#x9;:CONTROL.NBT_UNSUITABLE_AA IN ('2')&amp;#10;THEN&amp;#10;&amp;#10;&#x9;&#x9;:NPP.NPP_EMPLOYMENT_STATUS &#x9;:= 'EX';&amp;#10;&#x9;&#x9;:NPP.NPP_END_DATE&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:= SYSDATE;&amp;#10;&amp;#10;END IF;&amp;#10;&amp;#10;SELECT SYSDATE,&amp;#10;       USER&amp;#10;INTO   :NPP.NPP_LAST_UPDATE_DATE,&amp;#10;       :NPP.NPP_LAST_UPDATE_USER&amp;#10;FROM   DUAL;&amp;#10;&amp;#10;--wi1358&amp;#10;PR_RECALCULATE_PAYGROUP;&amp;#10;&amp;#10;&amp;#10;&amp;#10;"/>
         <Trigger Name="POST-QUERY" TriggerText="DECLARE P_OUT_CENTRE_NAME VARCHAR2(30);&amp;#10;&amp;#10;BEGIN&amp;#10; IF &#x9;:NPP.NPP_CENTRE_ID IS NOT NULL&amp;#10; THEN&amp;#10;&#x9;P_OUT_CENTRE_NAME := PK_NPP.FN_CENTRE_VALIDATION(:NPP.NPP_CENTRE_ID);&amp;#10;&#x9;:NPP.NBT_CENTRE_NAME := P_OUT_CENTRE_NAME;&amp;#10; END IF;&amp;#10;END;&amp;#10;&amp;#10;PR_CALCULATE_AGE;&amp;#10;&amp;#10;:CONTROL.NBT_UNSUITABLE_AA := NULL;&amp;#10;&amp;#10;IF&#x9;:NPP.NPP_DATE_OF_BIRTH IS NOT NULL&amp;#10;THEN&amp;#10;&#x9;BEGIN&amp;#10;&#x9; SELECT&#x9;DECODE(TO_CHAR(:NPP.NPP_DATE_OF_BIRTH,'MM'),&amp;#10;&#x9;&#x9; '08',TO_DATE('31/07/'||TO_CHAR(ADD_MONTHS(:NPP.NPP_DATE_OF_BIRTH,852),'YYYY'),&amp;#10;&#x9;&#x9;&#x9;'DD/MM/YYYY'),&amp;#10;&#x9;&#x9; '09',TO_DATE('31/07/'||TO_CHAR(ADD_MONTHS(:NPP.NPP_DATE_OF_BIRTH,852),'YYYY'),&amp;#10;&#x9;&#x9;&#x9;'DD/MM/YYYY'),&amp;#10;&#x9;&#x9; '10',TO_DATE('31/07/'||TO_CHAR(ADD_MONTHS(:NPP.NPP_DATE_OF_BIRTH,852),'YYYY'),&amp;#10;&#x9;&#x9;&#x9;'DD/MM/YYYY'),&amp;#10;&#x9;&#x9; '11',TO_DATE('31/07/'||TO_CHAR(ADD_MONTHS(:NPP.NPP_DATE_OF_BIRTH,852),'YYYY'),&amp;#10;&#x9;&#x9;&#x9;'DD/MM/YYYY'),&amp;#10;&#x9;&#x9; '12',TO_DATE('31/07/'||TO_CHAR(ADD_MONTHS(:NPP.NPP_DATE_OF_BIRTH,852),'YYYY'),&amp;#10;&#x9;&#x9;&#x9;'DD/MM/YYYY'),&amp;#10;&#x9;&#x9;      TO_DATE('31/07/'||TO_CHAR(ADD_MONTHS(:NPP.NPP_DATE_OF_BIRTH,840),'YYYY'),&amp;#10;&#x9;&#x9;&#x9;'DD/MM/YYYY'))&amp;#10;&#x9; INTO&#x9;:NPP.NBT_AS400_RETIREMENT_DATE&amp;#10;&#x9; FROM &#x9;DUAL;&amp;#10;&#x9;END;&amp;#10;END IF;&amp;#10;&amp;#10;&amp;#10;&amp;#10;--EC1739&amp;#10;:NPP.COPY_NPP_HMRC_Q1 := :NPP.NPP_HMRC_Q1;&amp;#10;:NPP.COPY_NPP_HMRC_Q2 := :NPP.NPP_HMRC_Q2;&amp;#10;--WI1358&amp;#10;:NPP.COPY_NPP_HMRC_INT_Q3 := :NPP.NPP_HMRC_INT_Q3;&amp;#10;:NPP.COPY_NPP_HMRC_INT_Q4 := :NPP.NPP_HMRC_INT_Q4;&amp;#10;:NPP.COPY_NPP_HMRC_INT_Q5 := :NPP.NPP_HMRC_INT_Q5;&amp;#10;:NPP.COPY_NPP_HMRC_INT_Q6 := :NPP.NPP_HMRC_INT_Q6;&amp;#10;:NPP.COPY_NPP_HMRC_INT_Q7 := :NPP.NPP_HMRC_INT_Q7;&amp;#10;:NPP.COPY_NPP_HMRC_INT_Q8 := :NPP.NPP_HMRC_INT_Q8;&amp;#10;&amp;#10;--EC1922&amp;#10;:NPP.COPY_NPP_NPAY_ID := :NPP.NPP_NPAY_ID;&amp;#10;&amp;#10;DECLARE&amp;#10;&#x9;CURSOR c_hmrc_int IS&amp;#10;&#x9;  SELECT NADD_CIA2_ID&amp;#10;&#x9;  FROM  NPP_ADDRESSES&amp;#10;&#x9;  WHERE NADD_NPP_ID = :NPP.NPP_ID&amp;#10;&#x9;  AND   NADD_ATYP_CODE = 'H';&amp;#10;&#x9; &amp;#10;&#x9;  &amp;#10;&#x9;v_nadd_cia2_id npp_addresses.nadd_cia2_id%type := null;&amp;#10;&#x9;&amp;#10;BEGIN&amp;#10;&#x9;v_nadd_cia2_id := null;&amp;#10;&#x9;&amp;#10;&#x9;OPEN c_hmrc_int;&amp;#10;&#x9;FETCH c_hmrc_int INTO v_nadd_cia2_id;&amp;#10;&#x9;CLOSE c_hmrc_int;&amp;#10;&#x9;&amp;#10;  :NPP.COPY_NPP_CIA2_ID := v_nadd_cia2_id;&amp;#10;  &amp;#10;  IF v_nadd_cia2_id = 'GB' &amp;#10;  THEN&amp;#10;    IF  :NPP.NPP_NPAY_ID IS NULL THEN&amp;#10;    &#x9;&amp;#10;&#x9;   SET_ITEM_PROPERTY('NPP.NPP_NPAY_ID',UPDATEABLE,PROPERTY_TRUE);&amp;#10;     SET_ITEM_PROPERTY('NPP.NPP_NPAY_ID',INSERT_ALLOWED,PROPERTY_TRUE);&amp;#10;    ELSE&amp;#10;    &#x9;SET_ITEM_PROPERTY('NPP.NPP_NPAY_ID',UPDATEABLE,PROPERTY_FALSE);&amp;#10;      SET_ITEM_PROPERTY('NPP.NPP_NPAY_ID',INSERT_ALLOWED,PROPERTY_FALSE);&amp;#10;    END IF;&amp;#10;  ELSE&amp;#10;     SET_ITEM_PROPERTY('NPP.NPP_NPAY_ID',UPDATEABLE,PROPERTY_FALSE);&amp;#10;     SET_ITEM_PROPERTY('NPP.NPP_NPAY_ID',INSERT_ALLOWED,PROPERTY_FALSE);  &#x9;&amp;#10;  END IF;&amp;#10;  &amp;#10;&amp;#10;&#x9;&amp;#10;&amp;#10;     SET_ITEM_PROPERTY('CONTROL.HMRC_Q_BUTTON',DISPLAYED,PROPERTY_TRUE);&amp;#10;     SET_ITEM_PROPERTY('CONTROL.HMRC_Q_BUTTON',ENABLED,PROPERTY_TRUE);&amp;#10;     SYNCHRONIZE;&amp;#10;&#x9;&amp;#10;&#x9;IF v_nadd_cia2_id &lt;> 'GB' THEN&amp;#10;&#x9; &#x9;&#x9;/*IF   :NPP.NPP_HMRC_INT_Q6 IS NULL OR&amp;#10;     &#x9;  :NPP.NPP_HMRC_INT_Q7 IS NULL OR&amp;#10;     &#x9;  :NPP.NPP_HMRC_INT_Q8 IS NULL OR&amp;#10;     &#x9;  :NPP.NPP_HMRC_INT_Q3 IS NULL OR&amp;#10;     &#x9;  :NPP.NPP_HMRC_INT_Q4 IS NULL OR&amp;#10;     &#x9;  :NPP.NPP_HMRC_INT_Q5 IS NULL*/&amp;#10;&#x9;&#x9; IF :NPP.NPP_HMRC_INT_Q6 IS NULL OR&amp;#10;     &#x9;  :NPP.NPP_HMRC_INT_Q7 IS NULL&amp;#10;     THEN&amp;#10;       &#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9; 'WARNING...some INTERNATIONAL HMRC reponses are missing...'||&amp;#10;                 '...Use ''HMRC Qs'' button.');&amp;#10;&amp;#10;&#x9;--&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&amp;#10;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;  &#x9;THEN &amp;#10;  &#x9; NULL;&amp;#10;&#x9;&#x9;END IF;&amp;#10;     &amp;#10;&#x9; END IF;&amp;#10;&#x9;END IF;&amp;#10;&#x9; &amp;#10;&#x9;:CONTROL.NBT_HOME_CIA2 := v_nadd_cia2_id;&amp;#10;&amp;#10;EXCEPTION&amp;#10;&#x9;&#x9;WHEN NO_DATA_FOUND THEN NULL;&amp;#10;END;&amp;#10;&#x9;&amp;#10;&#x9;declare&amp;#10;&#x9;cursor c_npay is&amp;#10;  SELECT NPAY_DESCRIPTION&amp;#10;  FROM NPP_PAYGROUPS&amp;#10;  WHERE NPAY_ID = :NPP.NPP_NPAY_ID;&amp;#10;  &amp;#10;begin&amp;#10;&#x9;&amp;#10;&#x9;open c_npay;&amp;#10;&#x9;fetch c_npay into :NPP.NBT_NPAY_DESCRIPTION;&amp;#10;&#x9;close c_npay;&amp;#10;&#x9;&amp;#10;exception&amp;#10;&#x9; when no_data_found then null;&amp;#10;end;&amp;#10;&amp;#10;&amp;#10;&amp;#10;"/>
         <Trigger Name="PRE-QUERY" TriggerText="if &#x9;:parameter.npp_id is not null&amp;#10;then &#x9;:npp.npp_id := :parameter.npp_id;&amp;#10;end if;&amp;#10;&amp;#10;:control.npp_surname &#x9;&#x9;:= :npp.npp_surname;&amp;#10;:control.npp_title&#x9;&#x9;:= :npp.npp_title;&amp;#10;:control.npp_initials&#x9;&#x9;:= :npp.npp_initials;&amp;#10;:control.npp_forenames&#x9;&#x9;:= :npp.npp_forenames;&amp;#10;:control.npp_gender&#x9;&#x9;:= :npp.npp_gender;&amp;#10;:control.npp_date_of_birth&#x9;:= :npp.npp_date_of_birth;"/>
         <Trigger Name="KEY-ENTQRY" TriggerText="ENTER_QUERY;&amp;#10;&amp;#10;--wi1358&amp;#10;--IF :NADD.NADD_CIA2_ID IS NULL OR&amp;#10;--&#x9; :NADD.NADD_CIA2_ID &lt;> 'GB'&amp;#10;--THEN&amp;#10;--&#x9;SET_ITEM_PROPERTY('CONTROL.HMRC_Q_BUTTON',DISPLAYED,PROPERTY_FALSE);&amp;#10;--END IF;&amp;#10;&#x9;&amp;#10;IF &#x9;:SYSTEM.LAST_RECORD = 'FALSE' AND&amp;#10;&#x9;:CONTROL.NPP_SURNAME IS NOT NULL&amp;#10;THEN&#x9;GO_BLOCK('NPP2');&amp;#10;        EXECUTE_QUERY;&amp;#10;END IF;"/>
         <Trigger Name="KEY-COMMIT" TriggerText="IF&#x9;:NPP.ROWID IS NULL AND&amp;#10;&#x9;:NADD.NADD_ADDRESS_LINE_1 IS NULL&amp;#10;THEN&amp;#10;&#x9;MSG_ALERT('PLEASE ENTER ADDRESS DETAILS BEFORE SAVING THIS NEW AA/NPP','E',TRUE);&amp;#10;ELSE&amp;#10;&#x9;--EC2115&amp;#10;&#x9;IF :NADD.NADD_ATYP_CODE = 'H' AND &amp;#10;&#x9;&#x9; :NADD.NADD_CIA2_ID IS NOT NULL &amp;#10;  THEN&amp;#10;&#x9;   DECLARE&amp;#10;&#x9;       CURSOR c_hmrc_int IS&amp;#10;&#x9;       SELECT NADD_CIA2_ID&amp;#10;&#x9;       FROM  NPP_ADDRESSES&amp;#10;&#x9;       WHERE NADD_NPP_ID = :NPP.NPP_ID&amp;#10;&#x9;       AND   NADD_ATYP_CODE = 'H';&amp;#10;&#x9;  &amp;#10;         v_old_nadd_cia2_id npp_addresses.nadd_cia2_id%type := null;&amp;#10;&amp;#10;     BEGIN&amp;#10;         v_old_nadd_cia2_id := null;&amp;#10;&amp;#10;&#x9;       OPEN c_hmrc_int;&amp;#10;&#x9;       FETCH c_hmrc_int INTO v_old_nadd_cia2_id;&amp;#10;&#x9;       CLOSE c_hmrc_int;&amp;#10;&#x9;&amp;#10;&#x9;&#x9;    &amp;#10;&#x9;&#x9;     IF&#x9;  :NADD.NADD_CIA2_ID &lt;> 'GB' &amp;#10;&#x9;&#x9;     THEN&amp;#10;&#x9;&#x9;        IF  :NPP.NPP_HMRC_INT_Q3 IS NULL AND&amp;#10;&#x9;&#x9;            :NPP.NPP_HMRC_INT_Q4 IS NULL AND&amp;#10;&#x9;&#x9;            :NPP.NPP_HMRC_INT_Q5 IS NULL AND &amp;#10;&#x9;              :NPP.NPP_HMRC_INT_Q6 IS NULL AND&amp;#10;&#x9;              :NPP.NPP_HMRC_INT_Q7 IS NULL AND&amp;#10;&#x9;&#x9;            :NPP.NPP_HMRC_INT_Q8 IS NULL&amp;#10;&#x9;&#x9;        THEN&amp;#10;&#x9;&#x9;           SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9;           'ERROR...answers to ''INTERNATIONAL HMRC QUESTIONS'' required...'||&amp;#10;                      '...Use ''HMRC Qs'' button.');&amp;#10;&amp;#10;               IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;           THEN &amp;#10;                  RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;           END IF;&amp;#10;&#x9;         END IF; &amp;#10;&#x9;&#x9;       &amp;#10;&#x9;&#x9;       IF :NADD.NADD_CIA2_ID &lt;> v_old_nadd_cia2_id &amp;#10;&#x9;&#x9;       THEN&amp;#10;&#x9;&#x9;  &#x9;     PR_RECALCULATE_PAYGROUP;&#x9;&amp;#10;&#x9;&#x9;         COMMIT_FORM;&amp;#10;&#x9;&#x9;       END IF;&amp;#10;&#x9;&#x9;    &amp;#10;&#x9;&#x9;     ELSIF :NADD.NADD_CIA2_ID = 'GB'&amp;#10;&#x9;&#x9;     THEN  &#x9;&amp;#10;&#x9;&#x9;       IF  :NADD.NADD_CIA2_ID &lt;> v_old_nadd_cia2_id &amp;#10;&#x9;&#x9;       THEN&amp;#10;&#x9;&#x9;         :NPP.NPP_HMRC_INT_Q3 := NULL;&amp;#10;&#x9;           :NPP.NPP_HMRC_INT_Q4 := NULL;&amp;#10;&#x9;&#x9;         :NPP.NPP_HMRC_INT_Q5 := NULL; &amp;#10;&#x9;           :NPP.NPP_HMRC_INT_Q6 := NULL;&amp;#10;&#x9;           :NPP.NPP_HMRC_INT_Q7 := NULL;&amp;#10;&#x9;&#x9;         :NPP.NPP_HMRC_INT_Q8 := NULL;&amp;#10;&#x9;&#x9;         &amp;#10;&#x9;&#x9;  &#x9;     PR_RECALCULATE_PAYGROUP;&#x9;&amp;#10;&#x9;&#x9;         COMMIT_FORM;&amp;#10;&#x9;&#x9;       END IF; &amp;#10;&#x9;&#x9;     END IF;&amp;#10;&#x9;&#x9; END;&amp;#10;  END IF;  --EC2115&amp;#10;     &amp;#10;&#x9;COMMIT_FORM;&amp;#10;END IF;"/>
      </Block>
