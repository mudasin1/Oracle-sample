      <ProgramUnit Name="CHECK_PACKAGE_FAILURE" ProgramUnitText="Procedure Check_Package_Failure IS&amp;#10;BEGIN&amp;#10;  IF NOT ( Form_Success ) THEN&amp;#10;    RAISE Form_Trigger_Failure;&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="QUERY_MASTER_DETAILS" ProgramUnitText="PROCEDURE Query_Master_Details(rel_id Relation,detail VARCHAR2) IS&amp;#10;  oldmsg VARCHAR2(2);  -- Old Message Level Setting&amp;#10;  reldef VARCHAR2(5);  -- Relation Deferred Setting&amp;#10;BEGIN&amp;#10;  --&amp;#10;  -- Initialize Local Variable(s)&amp;#10;  --&amp;#10;  reldef := Get_Relation_Property(rel_id, DEFERRED_COORDINATION);&amp;#10;  oldmsg := :System.Message_Level;&amp;#10;  --&amp;#10;  -- If NOT Deferred, Goto detail and execute the query.&amp;#10;  --&amp;#10;  IF reldef = 'FALSE' THEN&amp;#10;    Go_Block(detail);&amp;#10;    Check_Package_Failure;&amp;#10;    :System.Message_Level := '10';&amp;#10;    Execute_Query;&amp;#10;    :System.Message_Level := oldmsg;&amp;#10;  ELSE&amp;#10;    --&amp;#10;    -- Relation is deferred, mark the detail block as un-coordinated&amp;#10;    --&amp;#10;    Set_Block_Property(detail, COORDINATION_STATUS, NON_COORDINATED);&amp;#10;  END IF;&amp;#10;&amp;#10;EXCEPTION&amp;#10;    WHEN Form_Trigger_Failure THEN&amp;#10;      :System.Message_Level := oldmsg;&amp;#10;      RAISE;&amp;#10;END Query_Master_Details;&amp;#10;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="CLEAR_ALL_MASTER_DETAILS" ProgramUnitText="PROCEDURE Clear_All_Master_Details IS&amp;#10;  mastblk  VARCHAR2(30);  -- Initial Master Block Cusing Coord&amp;#10;  coordop  VARCHAR2(30);  -- Operation Causing the Coord&amp;#10;  trigblk  VARCHAR2(30);  -- Cur Block On-Clear-Details Fires On&amp;#10;  startitm VARCHAR2(61);  -- Item in which cursor started&amp;#10;  frmstat  VARCHAR2(15);  -- Form Status&amp;#10;  curblk   VARCHAR2(30);  -- Current Block&amp;#10;  currel   VARCHAR2(30);  -- Current Relation&amp;#10;  curdtl   VARCHAR2(30);  -- Current Detail Block&amp;#10;&amp;#10;  FUNCTION First_Changed_Block_Below(Master VARCHAR2)&amp;#10;  RETURN VARCHAR2 IS&amp;#10;    curblk VARCHAR2(30);  -- Current Block&amp;#10;    currel VARCHAR2(30);  -- Current Relation&amp;#10;    retblk VARCHAR2(30);  -- Return Block&amp;#10;  BEGIN&amp;#10;    --&amp;#10;    -- Initialize Local Vars&amp;#10;    --&amp;#10;    curblk := Master;&amp;#10;    currel := Get_Block_Property(curblk,  FIRST_MASTER_RELATION);&amp;#10;    --&amp;#10;    -- While there exists another relation for this block&amp;#10;    --&amp;#10;    WHILE currel IS NOT NULL LOOP&amp;#10;      --&amp;#10;      -- Get the name of the detail block&amp;#10;      --&amp;#10;      curblk := Get_Relation_Property(currel, DETAIL_NAME);&amp;#10;      --&amp;#10;      -- If this block has changes, return its name&amp;#10;      --&amp;#10;      IF ( Get_Block_Property(curblk, STATUS) IN('CHANGED','INSERT') ) THEN&amp;#10;        RETURN curblk;&amp;#10;      ELSE&amp;#10;        --&amp;#10;        -- No changes, recursively look for changed blocks below&amp;#10;        --&amp;#10;        retblk := First_Changed_Block_Below(curblk);&amp;#10;        --&amp;#10;        -- If some block below is changed, return its name&amp;#10;        --&amp;#10;        IF retblk IS NOT NULL THEN&amp;#10;          RETURN retblk;&amp;#10;        ELSE&amp;#10;          --&amp;#10;          -- Consider the next relation&amp;#10;          --&amp;#10;          currel := Get_Relation_Property(currel, NEXT_MASTER_RELATION);&amp;#10;        END IF;&amp;#10;      END IF;&amp;#10;    END LOOP;&amp;#10;&amp;#10;    --&amp;#10;    -- No changed blocks were found&amp;#10;    --&amp;#10;    RETURN NULL;&amp;#10;  END First_Changed_Block_Below;&amp;#10;&amp;#10;BEGIN&amp;#10;  --&amp;#10;  -- Init Local Vars&amp;#10;  --&amp;#10;  mastblk  := :System.Master_Block;&amp;#10;  coordop  := :System.Coordination_Operation;&amp;#10;  trigblk  := :System.Trigger_Block;&amp;#10;  startitm := :System.Trigger_Item;&amp;#10;  frmstat  := :System.Form_Status;&amp;#10;&amp;#10;  --&amp;#10;  -- If the coordination operation is anything but CLEAR_RECORD or&amp;#10;  -- SYNCHRONIZE_BLOCKS, then continue checking.&amp;#10;  --&amp;#10;  IF coordop NOT IN ('CLEAR_RECORD', 'SYNCHRONIZE_BLOCKS') THEN&amp;#10;    --&amp;#10;    -- If we're processing the driving master block...&amp;#10;    --&amp;#10;    IF mastblk = trigblk THEN&amp;#10;      --&amp;#10;      -- If something in the form is changed, find the&amp;#10;      -- first changed block below the master&amp;#10;      --&amp;#10;      IF frmstat = 'CHANGED' THEN&amp;#10;        curblk := First_Changed_Block_Below(mastblk);&amp;#10;        --&amp;#10;        -- If we find a changed block below, go there&amp;#10;        -- and Ask to commit the changes.&amp;#10;        --&amp;#10;        IF curblk IS NOT NULL THEN&amp;#10;          Go_Block(curblk);&amp;#10;          Check_Package_Failure;&amp;#10;          Clear_Block(ASK_COMMIT);&amp;#10;          --&amp;#10;          -- If user cancels commit dialog, raise error&amp;#10;          --&amp;#10;          IF NOT ( :System.Form_Status = 'QUERY'&amp;#10;                   OR :System.Block_Status = 'NEW' ) THEN&amp;#10;            RAISE Form_Trigger_Failure;&amp;#10;          END IF;&amp;#10;        END IF;&amp;#10;      END IF;&amp;#10;    END IF;&amp;#10;  END IF;&amp;#10;&amp;#10;  --&amp;#10;  -- Clear all the detail blocks for this master without&amp;#10;  -- any further asking to commit.&amp;#10;  --&amp;#10;  currel := Get_Block_Property(trigblk, FIRST_MASTER_RELATION);&amp;#10;  WHILE currel IS NOT NULL LOOP&amp;#10;    curdtl := Get_Relation_Property(currel, DETAIL_NAME);&amp;#10;    IF Get_Block_Property(curdtl, STATUS) &lt;> 'NEW'  THEN&amp;#10;      Go_Block(curdtl);&amp;#10;      Check_Package_Failure;&amp;#10;      Clear_Block(NO_VALIDATE);&amp;#10;      IF :System.Block_Status &lt;> 'NEW' THEN&amp;#10;        RAISE Form_Trigger_Failure;&amp;#10;      END IF;&amp;#10;    END IF;&amp;#10;    currel := Get_Relation_Property(currel, NEXT_MASTER_RELATION);&amp;#10;  END LOOP;&amp;#10;&amp;#10;  --&amp;#10;  -- Put cursor back where it started&amp;#10;  --&amp;#10;  IF :System.Cursor_Item &lt;> startitm THEN&amp;#10;    Go_Item(startitm);&amp;#10;    Check_Package_Failure;&amp;#10;  END IF;&amp;#10;&amp;#10;EXCEPTION&amp;#10;  WHEN Form_Trigger_Failure THEN&amp;#10;    IF :System.Cursor_Item &lt;> startitm THEN&amp;#10;      Go_Item(startitm);&amp;#10;    END IF;&amp;#10;    RAISE;&amp;#10;&amp;#10;END Clear_All_Master_Details;&amp;#10;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_FORMAT_NAME" ProgramUnitText="PROCEDURE PR_FORMAT_NAME IS&amp;#10;BEGIN&amp;#10;     :NPP.NPP_NAME := :NPP.NPP_TITLE||' '||&amp;#10;                      :NPP.NPP_INITIALS||' '||&amp;#10;&#x9;&#x9;      :NPP.NPP_SURNAME;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_FORMAT_NPP_BUTTONS" ProgramUnitText="PROCEDURE PR_FORMAT_NPP_BUTTONS IS&amp;#10;BEGIN&amp;#10;  IF :NQUA.ROWID IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.NQUA_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.NQUA_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;  &amp;#10;  IF :NROL.ROWID IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.NROL_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.NROL_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;&amp;#10;  IF :NLAN.ROWID IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.NLAN_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.NLAN_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;  &amp;#10;  IF :NASU.ROWID IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.NASU_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.NASU_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;  &amp;#10;  IF :NVSE.ROWID IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.NVSE_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.NVSE_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;&amp;#10;  IF :NPP.NPP_BANK_ACCOUNT_NO IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.BANK_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.BANK_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;&amp;#10;  IF :NPP.NPP_ID IS NULL&amp;#10;  THEN&amp;#10;   DISPLAY_ITEM('CONTROL.PROFILE_BUTTON','CG$PUSH_BUTTON');&amp;#10;  ELSE&amp;#10;   DISPLAY_ITEM('CONTROL.PROFILE_BUTTON','BUTTON_WITH_DETAIL');&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NQUA" ProgramUnitText="PROCEDURE PR_VALIDATE_NQUA IS&amp;#10;BEGIN&amp;#10;  IF :NQUA.NQUA_QCOD_ID &#x9;IS NULL AND&amp;#10;     :NQUA.NQUA_DESCRIPTION &#x9;IS NULL&amp;#10;  THEN&amp;#10;   &#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'ERROR...QUALIFICATION CODE OR FREE FORMAT TEXT DESCRIPTION MUST BE INPUT');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_CALCULATE_NVSE" ProgramUnitText="PROCEDURE PR_CALCULATE_NVSE IS&amp;#10;BEGIN&amp;#10;  IF &#x9;:NVSE.NVSE_FIRST_IND = 'Y'&amp;#10;  THEN &#x9;:NVSE.NVSE_MINIMUM_AWARD_CODE := '01';&amp;#10;  ELSE&#x9;IF&#x9;:NVSE.NVSE_NATIONAL_IND = 'Y'&amp;#10;&#x9;THEN&#x9;:NVSE.NVSE_MINIMUM_AWARD_CODE := '03';&amp;#10;  &#x9;ELSE&#x9;IF&#x9;:NVSE.NVSE_HIGHER_IND = 'Y'&amp;#10;&#x9;&#x9;THEN&#x9;:NVSE.NVSE_MINIMUM_AWARD_CODE := '05';&amp;#10;&#x9;&#x9;ELSE&#x9;:NVSE.NVSE_MINIMUM_AWARD_CODE := NULL;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;  END IF;&amp;#10;  IF &#x9;:NVSE.NVSE_HIGHER_IND = 'Y'&amp;#10;  THEN &#x9;:NVSE.NVSE_MAXIMUM_AWARD_CODE := '11';&amp;#10;  ELSE&#x9;IF&#x9;:NVSE.NVSE_NATIONAL_IND = 'Y'&amp;#10;&#x9;THEN&#x9;:NVSE.NVSE_MAXIMUM_AWARD_CODE := '04';&amp;#10;  &#x9;ELSE&#x9;IF&#x9;:NVSE.NVSE_FIRST_IND = 'Y'&amp;#10;&#x9;&#x9;THEN&#x9;:NVSE.NVSE_MAXIMUM_AWARD_CODE := '02';&amp;#10;&#x9;&#x9;ELSE&#x9;:NVSE.NVSE_MAXIMUM_AWARD_CODE := NULL;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_GET_ROLE_TYPE_DETAIL" ProgramUnitText="PROCEDURE PR_GET_ROLE_TYPE_DETAIL IS&amp;#10;BEGIN&amp;#10;  SELECT NRTY_CONTACTS_IND,&amp;#10;         NRTY_MODERATORS_IND,&amp;#10;         NRTY_GUNI_EXCLUSIONS_IND,&amp;#10;         NRTY_TEAMS_IND,&amp;#10;         NRTY_SUBJECT_LEVELS_IND,&amp;#10;         NRTY_BUSINESS_UNITS_IND,&amp;#10;         NRTY_CENTRE_EXCLUSIONS_IND,&amp;#10;         NRTY_AS400_IND,&amp;#10;         NRTY_NRWA_MAINTAINABLE_IND,&amp;#10;         NRTY_NRWA_DATE_TYPE,&amp;#10;         NRTY_NRWA_DETAIL_TYPE,&amp;#10;         NRTY_REGIONS_IND,&amp;#10;         NVL(NRTY_VOCATIONAL_IND,'N')&amp;#10;  INTO   :NROL.NBT_NRTY_CONTACTS_IND,&amp;#10;         :NROL.NBT_NRTY_MODERATORS_IND,&amp;#10;         :NROL.NBT_NRTY_GUNI_EXCLUSIONS_IND,&amp;#10;         :NROL.NBT_NRTY_TEAMS_IND,&amp;#10;         :NROL.NBT_NRTY_SUBJECT_LEVELS_IND,&amp;#10;         :NROL.NBT_NRTY_BUSINESS_UNITS_IND,&amp;#10;         :NROL.NBT_NRTY_CENTRE_EXCLUSIONS_IND,&amp;#10;         :NROL.NBT_NRTY_AS400_IND,&amp;#10;         :NROL.NBT_NRTY_NRWA_MAINTAINABLE_IND,&amp;#10;         :NROL.NBT_NRTY_NRWA_DATE_TYPE,&amp;#10;         :NROL.NBT_NRTY_NRWA_DETAIL_TYPE,&amp;#10;         :NROL.NBT_NRTY_REGIONS_IND,&amp;#10;         :NROL.NBT_NRTY_VOCATIONAL_IND&amp;#10;  FROM   NPP_ROLE_TYPES&amp;#10;  WHERE  NRTY_ID = :NROL.NROL_NRTY_ID;&amp;#10;  EXCEPTION&amp;#10;  WHEN NO_DATA_FOUND THEN NULL;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_NROL_STACKING" ProgramUnitText="PROCEDURE PR_NROL_STACKING IS&amp;#10;  Y_POSITION NUMBER(5,3);&amp;#10;  X_POSITION NUMBER(5,3);&amp;#10;BEGIN&amp;#10;  IF   :NROL.NBT_NRTY_MODERATORS_IND = 'Y' AND&amp;#10;       :NROL.NROL_NRTY_ID &lt;> 1&amp;#10;  THEN SHOW_VIEW('NROL_CANVAS1');&amp;#10;       SET_ITEM_PROPERTY('NROL.NBT_NPP_MODREG_M_CODE',ENABLED,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NBT_NPP_MODREG_M_MODERATOR_ID',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  ELSE HIDE_VIEW('NROL_CANVAS1');&amp;#10;       SET_ITEM_PROPERTY('NROL.NBT_NPP_MODREG_M_CODE',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NBT_NPP_MODREG_M_MODERATOR_ID',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;  &amp;#10;  HIDE_VIEW('NROL_CANVAS2');&amp;#10;  SET_ITEM_PROPERTY('NROL.NROL_ENTRY_MAX_WORKLOAD',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  IF   :NROL.NROL_NRTY_ID IN (121)&amp;#10;  THEN SHOW_VIEW('NROL_CANVAS8');&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_MAX_SCRIPTS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_SERIES_ID',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_MAX_SCRIPTS_2',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_SERIES_ID_2',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  ELSE HIDE_VIEW('NROL_CANVAS8');&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_MAX_SCRIPTS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_SERIES_ID',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_MAX_SCRIPTS_2',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_SERIES_ID_2',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;  &amp;#10;-- IF   :NROL.NROL_NRTY_ID IN (1,106,108,110,114,122,127,154,165,166,171,147,152,153,300,216,217,200,526,551)&amp;#10;  IF   :NROL.NBT_NRTY_VOCATIONAL_IND = 'Y'&amp;#10;  THEN SHOW_VIEW('NROL_CANVAS6');&amp;#10;       SET_ITEM_PROPERTY('NROL.NROL_MAX_CENTRES',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  ELSE HIDE_VIEW('