            <Trigger Name="WHEN-VALIDATE-ITEM" TriggerText="--EC2121 ONLY VALIDATE IF COUNTRY CODE IS 'GB'&amp;#10;IF :NADD.NADD_POSTCODE IS NOT NULL AND&amp;#10;   NVL(LENGTH(:NADD.NADD_POSTCODE), 0) > 5 AND&amp;#10;   :NADD.NADD_CIA2_ID = 'GB'&amp;#10;THEN&amp;#10;&amp;#10;IF &#x9;SUBSTR(:NADD.NADD_POSTCODE,1,1) NOT BETWEEN 'A' AND 'Z'&amp;#10;   OR  (SUBSTR(:NADD.NADD_POSTCODE,2,1) NOT BETWEEN '0' AND '9'&amp;#10;   AND  SUBSTR(:NADD.NADD_POSTCODE,3,1) NOT BETWEEN '0' AND '9')&amp;#10;   OR   SUBSTR(:NADD.NADD_POSTCODE,&amp;#10;          INSTR(:NADD.NADD_POSTCODE,' ')+1,1)&amp;#10;          NOT BETWEEN '0' AND '9'&amp;#10;   OR   SUBSTR(:NADD.NADD_POSTCODE,&amp;#10;          INSTR(:NADD.NADD_POSTCODE,' ')+2,1)&amp;#10;          NOT BETWEEN 'A' AND 'Z'&amp;#10;   OR   SUBSTR(:NADD.NADD_POSTCODE,&amp;#10;          INSTR(:NADD.NADD_POSTCODE,' ')+3,1)&amp;#10;          NOT BETWEEN 'A' AND 'Z'&amp;#10;   OR   SUBSTR(:NADD.NADD_POSTCODE||'*',&amp;#10;          INSTR(:NADD.NADD_POSTCODE,' ')+4,1)&amp;#10;          &lt;> '*'&amp;#10;THEN &#x9;&amp;#10;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;         'INVALID POSTCODE FORMAT...PLEASE RE-ENTER');&amp;#10;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;&#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;END IF;&amp;#10;END IF;&amp;#10;&amp;#10;:NADD.NADD_GRID_REFERENCE_EAST  := NULL;&amp;#10;:NADD.NADD_GRID_REFERENCE_NORTH := NULL;&amp;#10; &amp;#10;BEGIN&amp;#10;&amp;#10; SELECT&#x9;GREF_EAST,&amp;#10;&#x9;GREF_NORTH&amp;#10; INTO&#x9;:NADD.NADD_GRID_REFERENCE_EAST,&amp;#10;&#x9;:NADD.NADD_GRID_REFERENCE_NORTH&amp;#10; FROM&#x9;GRID_REFERENCES&amp;#10; WHERE&#x9;GREF_POSTCODE = SUBSTR(:NADD.NADD_POSTCODE,1,6);&amp;#10;&amp;#10; EXCEPTION&amp;#10; WHEN NO_DATA_FOUND THEN&amp;#10;&amp;#10;&#x9;BEGIN&amp;#10;&amp;#10; &#x9; SELECT&#x9;GREF_EAST,&amp;#10;&#x9;&#x9;GREF_NORTH&amp;#10; &#x9; INTO&#x9;:NADD.NADD_GRID_REFERENCE_EAST,&amp;#10;&#x9;&#x9;:NADD.NADD_GRID_REFERENCE_NORTH&amp;#10; &#x9; FR