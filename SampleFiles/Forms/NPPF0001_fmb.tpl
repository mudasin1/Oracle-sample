      <Trigger Name="POST-FORMS-COMMIT" TriggerText="DECLARE RECORD_COUNT1 NUMBER(3);&amp;#10;        RECORD_COUNT2 NUMBER(3);&amp;#10;&amp;#10;BEGIN&amp;#10; &amp;#10; IF :NPP.NPP_ID IS NOT NULL&amp;#10; THEN&amp;#10;&amp;#10;&#x9;SELECT &#x9;COUNT(*), &amp;#10;&#x9;&#x9;SUM(DECODE(NADD_DEFAULT_IND,'Y',1,0))&amp;#10;&#x9;INTO&#x9;RECORD_COUNT1, &amp;#10;&#x9;&#x9;RECORD_COUNT2&amp;#10;&#x9;FROM&#x9;NPP_ADDRESSES&amp;#10;&#x9;WHERE&#x9;NADD_NPP_ID &#x9; = :NPP.NPP_ID;&amp;#10;&amp;#10;&#x9;IF&#x9;RECORD_COUNT1  = 0&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;        &#x9;&#x9;'ERROR...NPP MUST HAVE AT LEAST 1 ADDRESS');&amp;#10; &#x9;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10; &#x9;&#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10; &#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;RECORD_COUNT1  > 0 AND&amp;#10;&#x9;&#x9;RECORD_COUNT2 &lt;> 1&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;        &#x9;&#x9;'WARNING...NPP SHOULD HAVE 1 DEFAULT ADDRESS');&amp;#10; &#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10; &#x9;&#x9;THEN NULL;&amp;#10; &#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10; END IF;&amp;#10;&amp;#10;END;&amp;#10;&amp;#10;IF&#x9;:NPP.NPP_EMPLOYMENT_STATUS = 'CU' AND :NPP.NPP_ID IS NOT NULL&amp;#10;THEN&amp;#10;&amp;#10;&#x9;DECLARE RECORD_COUNT1 NUMBER(3);&amp;#10;&amp;#10;&#x9;BEGIN&amp;#10; &amp;#10;&#x9;&#x9;SELECT &#x9;COUNT(*)&amp;#10;&#x9;&#x9;INTO&#x9;RECORD_COUNT1&amp;#10;&#x9;&#x9;FROM&#x9;NPP_ROLES&amp;#10;&#x9;&#x9;WHERE&#x9;NROL_NPP_ID &#x9;       = :NPP.NPP_ID&amp;#10;&#x9;&#x9;AND&#x9;NROL_EMPLOYMENT_STATUS = 'CU';&amp;#10;&amp;#10;&#x9;&#x9;IF&#x9;RECORD_COUNT1  = 0&amp;#10;&#x9;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;        &#x9;&#x9;&#x9;'WARNING...NPP HAS NO CURRENT ROLE(S) DEFINED');&amp;#10; &#x9;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10; &#x9;&#x9;&#x9;THEN NULL;&amp;#10; &#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&amp;#10;&#x9;END;&amp;#10;&amp;#10;END IF;&amp;#10;&amp;#10;IF&#x9;:NPP.NPP_EMPLOYMENT_STATUS &lt;> 'CU' AND :NPP.NPP_ID IS NOT NULL&amp;#10;THEN&amp;#10;&amp;#10; DECLARE RECORD_COUNT1 NUMBER(3);&amp;#10; &amp;#10; BEGIN&amp;#10; &amp;#10;&#x9;SELECT &#x9;COUNT(*)&amp;#10;&#x9;INTO&#x9;RECORD_COUNT1&amp;#10;&#x9;FROM&#x9;NPP_ROLES&amp;#10;&#x9;WHERE&#x9;NROL_NPP_ID &#x9; &#x9;= :NPP.NPP_ID&amp;#10;&#x9;AND&#x9;NROL_EMPLOYMENT_STATUS&#x9;= 'CU';&amp;#10;&amp;#10;&#x9;IF&#x9;RECORD_COUNT1  = 1&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;        &#x9;&#x9;'1 CURRENT ROLE EXISTS FOR A NON-CURRENT NPP');&amp;#10; &#x9;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10; &#x9;&#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10; &#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;RECORD_COUNT1  > 1&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;        &#x9;&#x9;TO_CHAR(RECORD_COUNT1)||&amp;#10;        &#x9;&#x9;' CURRENT ROLES EXIST FOR A NON-CURRENT NPP');&amp;#10; &#x9;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10; &#x9;&#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10; &#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10; END;&amp;#10;&amp;#10;END IF;"/>
