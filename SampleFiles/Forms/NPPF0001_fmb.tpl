      <ProgramUnit Name="PR_INPUT_NOT_ALLOWED" ProgramUnitText="PROCEDURE PR_INPUT_NOT_ALLOWED IS&amp;#10;BEGIN&amp;#10;  &#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'DETAIL IS DISPLAY ONLY...NO INPUT ALLOWED');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_NRWA_STACKING" ProgramUnitText="PROCEDURE PR_NRWA_STACKING IS&amp;#10;  Y_POSITION NUMBER(5,3);&amp;#10;  X_POSITION NUMBER(5,3);&amp;#10;BEGIN&amp;#10;  Y_POSITION := 0.3;&amp;#10;  X_POSITION := 0.1;&amp;#10;  SHOW_VIEW('NRWA_CANVAS');&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DATE_TYPE = 'S'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS1');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_EXAM_SESSION',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_EXAM_LEVEL',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_EXAM_HOME_OVERSEAS_IND',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS1',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;       X_POSITION := X_POSITION + 2;&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS1');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_EXAM_SESSION',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_EXAM_LEVEL',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_EXAM_HOME_OVERSEAS_IND',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DATE_TYPE = 'A'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS2');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ACADEMIC_YEAR',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS2',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;       X_POSITION := X_POSITION + 1.4;&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS2');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ACADEMIC_YEAR',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DATE_TYPE = 'R'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS3');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_START_DATE',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_END_DATE',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS3',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;       X_POSITION := X_POSITION + 2;&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS3');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_START_DATE',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_END_DATE',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DATE_TYPE = 'M'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS4');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_MONTH',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS4',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;       X_POSITION := X_POSITION + 1.1;&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS4');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_MONTH',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DETAIL_TYPE = 'A'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS5');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_WORK_CONTRACTED',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_WORK_EXTENDED',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_WORKED',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_TRAINING',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_TRAINED',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS5',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS5');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_WORK_CONTRACTED',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_WORK_EXTENDED',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_WORKED',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_TRAINING',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_DAYS_TRAINED',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DETAIL_TYPE = 'B'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS6');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_APPLICATIONS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS6',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS6');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_APPLICATIONS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DETAIL_TYPE = 'C'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS7');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_PAPERS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_MARKED_PAPERS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS7',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS7');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_PAPERS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_MARKED_PAPERS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DETAIL_TYPE = 'D'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS8');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_CENTRES',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_SECTORS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_PROGRAMMES',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_UNITS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS8',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS8');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_CENTRES',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_SECTORS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_PROGRAMMES',NAVIGABLE,PROPERTY_FALSE);&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_UNITS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DETAIL_TYPE = 'E'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS9');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_TP_CONTRACTS',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS9',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS9');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_ALLOCATED_TP_CONTRACTS',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;  IF   :NROL.NBT_NRTY_NRWA_DETAIL_TYPE = 'F'&amp;#10;  THEN SHOW_VIEW('NRWA_CANVAS10');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_CENTRE_ID',NAVIGABLE,PROPERTY_TRUE);&amp;#10;       SET_VIEW_PROPERTY('NRWA_CANVAS10',DISPLAY_POSITION,X_POSITION,Y_POSITION);&amp;#10;  ELSE HIDE_VIEW('NRWA_CANVAS10');&amp;#10;       SET_ITEM_PROPERTY('NRWA.NRWA_CENTRE_ID',NAVIGABLE,PROPERTY_FALSE);&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_CALCULATE_AGE" ProgramUnitText="PROCEDURE PR_CALCULATE_AGE IS&amp;#10;BEGIN&amp;#10;  IF :NPP.NPP_DATE_OF_BIRTH IS NOT NULL&amp;#10;  THEN :NPP.NBT_AGE := TRUNC(MONTHS_BETWEEN(SYSDATE,:NPP.NPP_DATE_OF_BIRTH)/12);&amp;#10;  ELSE :NPP.NBT_AGE := NULL;&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NROL_DATES" ProgramUnitText="PROCEDURE PR_VALIDATE_NROL_DATES IS&amp;#10;BEGIN&amp;#10;&amp;#10;     IF&#x9;:NROL.NROL_START_DATE &lt; :NPP.NPP_START_DATE &#x9;AND &amp;#10;        :NROL.NROL_START_DATE IS NOT NULL&#x9;&#x9;AND&amp;#10;&#x9;:NPP.NPP_START_DATE   IS NOT NULL&amp;#10;     THEN&amp;#10; &amp;#10; &#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'NPP AND ROLE START DATES ARE INCOMPATIBLE');&amp;#10;&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;     END IF;&amp;#10;&amp;#10;     IF&#x9;:NROL.NROL_END_DATE   > :NPP.NPP_END_DATE &#x9;AND &amp;#10;        :NROL.NROL_END_DATE   IS NOT NULL&#x9;&#x9;AND&amp;#10;&#x9;:NPP.NPP_END_DATE     IS NOT NULL&amp;#10;     THEN&amp;#10; &amp;#10; &#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'NPP AND ROLE END DATES ARE INCOMPATIBLE');&amp;#10;&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;     END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NPP_STATUS" ProgramUnitText="PROCEDURE PR_VALIDATE_NPP_STATUS IS&amp;#10;BEGIN&amp;#10;  IF :NPP.NPP_EMPLOYMENT_STATUS = 'CU' AND&amp;#10;     :NPP.NPP_END_DATE IS NOT NULL&amp;#10;  THEN   &#x9;&amp;#10;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'END DATE INVALID FOR A CURRENT NPP');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;  END IF;&amp;#10;  IF :NPP.NPP_EMPLOYMENT_STATUS &lt;> 'CU' AND&amp;#10;     :NPP.NPP_END_DATE IS NULL&amp;#10;  THEN   &#x9;&amp;#10;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'NOT A CURRENT NPP - END DATE REQUIRED');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;  END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NROL_STATUS" ProgramUnitText="PROCEDURE PR_VALIDATE_NROL_STATUS IS&amp;#10;BEGIN&amp;#10;&amp;#10;  IF :NROL.NROL_EMPLOYMENT_STATUS IN ('CU','NR','ND','CE') AND&amp;#10;     :NROL.NROL_END_DATE IS NOT NULL&amp;#10;  THEN   &#x9;&amp;#10;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'END DATE INVALID FOR A CURRENT / NEWLY RECRUITED ROLE');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;  END IF;&amp;#10;&amp;#10;  IF :NROL.NROL_EMPLOYMENT_STATUS NOT IN ('CU','NR','ND','CE') AND&amp;#10;     :NROL.NROL_END_DATE IS NULL&amp;#10;  THEN   &#x9;&amp;#10;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'NOT A CURRENT ROLE - END DATE REQUIRED');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;  END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NROL_MODREG" ProgramUnitText="PROCEDURE PR_VALIDATE_NROL_MODREG IS&amp;#10;BEGIN&amp;#10;     IF&#x9;:NROL.NBT_NRTY_MODERATORS_IND = 'Y'&#x9;&#x9;AND&amp;#10;&#x9;:NROL.NROL_EMPLOYMENT_STATUS  = 'CU'&#x9;&#x9;AND&amp;#10;&#x9;:NROL.NBT_NPP_MODREG_M_MODERATOR_ID IS NULL&amp;#10;     THEN&amp;#10; &#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;           'A UNIQUE MODERATOR CODE IS MANDATORY FOR THIS TYPE OF ROLE');&amp;#10;&amp;#10;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;     END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_UPDATE_USER" ProgramUnitText="PROCEDURE pr_update_user IS&amp;#10;&#x9;role_set boolean;&amp;#10;BEGIN&amp;#10; :control.update_user := 'N';&amp;#10;&amp;#10; role_set := dbms_session.is_role_enabled('NPP_DML_USER');&amp;#10; if &#x9;role_set &amp;#10; then&#x9;&amp;#10;&#x9;:control.update_user := 'Y';&amp;#10; else&amp;#10;&#x9;role_set := dbms_session.is_role_enabled('NPP_ACC_QUERY_USER');&amp;#10;&#x9;if &#x9;role_set &amp;#10; &#x9;then&#x9;:control.update_user := 'Y';&amp;#10;&#x9;else&amp;#10; &#x9;&#x9;role_set := dbms_session.is_role_enabled('NPP_OWNER');&amp;#10; &#x9;&#x9;if &#x9;role_set &amp;#10; &#x9;&#x9;then&#x9;:control.update_user := 'Y';&amp;#10; &#x9;&#x9;end if;&amp;#10; &#x9;end if;&amp;#10;&amp;#10; end if;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_SELECT_NPP" ProgramUnitText="PROCEDURE PR_SELECT_NPP IS&amp;#10;BEGIN&amp;#10;&#x9;hide_window(:system.event_window);&amp;#10;&#x9;:PARAMETER.NPP_ID := :NPP2.NPP_ID;&amp;#10;&#x9;GO_BLOCK('NPP');&amp;#10;&#x9;CLEAR_BLOCK;&amp;#10;&#x9;EXECUTE_QUERY;&amp;#10;&#x9;:PARAMETER.NPP_ID := NULL;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_POPULATE_WORK_ADDRESS" ProgramUnitText="PROCEDURE PR_POPULATE_WORK_ADDRESS IS&amp;#10;BEGIN&amp;#10;&amp;#10;&#x9;IF&#x9;:NADD.NADD_ATYP_CODE&#x9;IS NULL&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;LAST_RECORD;&amp;#10;&#x9;&#x9;CREATE_RECORD;&amp;#10;&#x9;&#x9;:NADD.NADD_ATYP_CODE &#x9;&#x9;:= 'W';&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;:NADD.NADD_DEFAULT_IND&#x9;&#x9;:= 'N';&amp;#10;&amp;#10;&#x9;BEGIN&amp;#10;&amp;#10;&#x9; SELECT&#x9;CN_SHORT_NAME,&amp;#10;&#x9;&#x9;CN_ADDRESS_1,&amp;#10;&#x9;&#x9;CN_ADDRESS_2,&amp;#10;&#x9;&#x9;CN_TOWN,&amp;#10;&#x9;&#x9;CN_COUNTY,&amp;#10;&#x9;&#x9;CN_POSTCODE&amp;#10;&#x9; INTO&#x9;:NADD.NADD_ADDRESS_LINE_1,&amp;#10;&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_2,&amp;#10;&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_3,&amp;#10;&#x9;&#x9;:NADD.NADD_TOWN,&amp;#10;&#x9;&#x9;:NADD.NADD_COUNTY,&amp;#10;&#x9;&#x9;:NADD_POSTCODE&amp;#10;&#x9; FROM&#x9;CENTRES&amp;#10;&#x9; WHERE&#x9;CN_CENTRE_ID = :NPP.NPP_CENTRE_ID&amp;#10;&#x9; AND&#x9;CN_ADDRESS_3 IS NULL;&amp;#10;&amp;#10;&#x9; EXCEPTION WHEN NO_DATA_FOUND THEN&amp;#10;&amp;#10;&#x9;  BEGIN&amp;#10;&amp;#10;&#x9;   SELECT&#x9;CC_SHORTNAME,&amp;#10;&#x9;&#x9;&#x9;CC_STREET,&amp;#10;&#x9;&#x9;&#x9;CC_VILLAGE,&amp;#10;&#x9;&#x9;&#x9;CC_POSTTOWN,&amp;#10;&#x9;&#x9;&#x9;CC_POSTCOUNTY,&amp;#10;&#x9;&#x9;&#x9;CC_POSTCODE&amp;#10;&#x9;   INTO&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_1,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_2,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_3,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_TOWN,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_COUNTY,&amp;#10;&#x9;&#x9;&#x9;:NADD_POSTCODE&amp;#10;&#x9;   FROM&#x9;&#x9;CAMBRIDGE_CENTRES&amp;#10;&#x9;   WHERE&#x9;CC_CENTRE_ID = TO_NUMBER(SUBSTR(:NPP.NPP_CENTRE_ID,1,5))&amp;#10;&#x9;   AND&#x9;&#x9;NVL(LENGTH(:NPP.NPP_CENTRE_ID), 0) = 5;&amp;#10;&amp;#10;&#x9;   EXCEPTION WHEN NO_DATA_FOUND THEN&amp;#10;&amp;#10;&#x9;   BEGIN&amp;#10;&amp;#10;&#x9;    SELECT&#x9;UCEN_CENTRE_NAME,&amp;#10;&#x9;&#x9;&#x9;UCEN_ADDRESS_1,&amp;#10;&#x9;&#x9;&#x9;UCEN_ADDRESS_2,&amp;#10;&#x9;&#x9;&#x9;UCEN_ADDRESS_3,&amp;#10;&#x9;&#x9;&#x9;UCEN_ADDRESS_4,&amp;#10;&#x9;&#x9;&#x9;UCEN_POSTCODE&amp;#10;&#x9;    INTO&#x9;:NADD.NADD_ADDRESS_LINE_1,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_2,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_ADDRESS_LINE_3,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_TOWN,&amp;#10;&#x9;&#x9;&#x9;:NADD.NADD_COUNTY,&amp;#10;&#x9;&#x9;&#x9;:NADD_POSTCODE&amp;#10;&#x9;    FROM&#x9;ULEAC_CENTRES&amp;#10;&#x9;    WHERE&#x9;UCEN_CENTRE_ID = :NPP.NPP_CENTRE_ID;&amp;#10;&amp;#10;&#x9;    EXCEPTION WHEN NO_DATA_FOUND THEN NULL;&amp;#10;&amp;#10;&#x9;  END;&amp;#10;&amp;#10;&#x9; END;&amp;#10;&amp;#10;&#x9;END;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_GENDER_TITLE" ProgramUnitText="PROCEDURE PR_VALIDATE_GENDER_TITLE IS&amp;#10;BEGIN&amp;#10;  &amp;#10;&#x9;IF&#x9;:NPP.NPP_TITLE &#x9;IS NOT NULL AND&amp;#10;&#x9;&#x9;:NPP.NPP_GENDER IS NOT NULL&amp;#10;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;IF&#x9;(:NPP.NPP_TITLE &#x9;IN ('MR','SIR') &#x9;&#x9;AND&amp;#10;&#x9;&#x9;&#x9; :NPP.NPP_GENDER&#x9;= 'F')&#x9;&#x9;&#x9;&#x9;OR&amp;#10;&#x9;&#x9;&#x9;(:NPP.NPP_TITLE&#x9;&#x9;IN ('MISS','MS','MRS','LADY') &#x9;AND&amp;#10;&#x9;&#x9;&#x9; :NPP.NPP_GENDER&#x9;= 'M')&amp;#10;&#x9;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;          &#x9; &#x9;'NPP TITLE AND GENDER ARE INCONSISTENT');&amp;#10;&#x9;  &#x9;&#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;&#x9;&#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;&#x9;&#x9;END IF;&amp;#10;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_QASALLOC" ProgramUnitText="PROCEDURE PR_VALIDATE_QASALLOC IS&amp;#10;&#x9;P_DUMMY VARCHAR2(1);&amp;#10;BEGIN&amp;#10;&amp;#10;&#x9;SELECT&#x9;NULL&amp;#10;&#x9;INTO&#x9;P_DUMMY&amp;#10;&#x9;FROM&#x9;DUAL&amp;#10;&#x9;WHERE&#x9;EXISTS ( SELECT NULL&amp;#10;&#x9;&#x9;&#x9; FROM&#x9;VERIFICATION_SCHEDULE_RECORDS,&amp;#10;&#x9;&#x9;&#x9;&#x9;EV_ALLOCATIONS&#x9;&amp;#10;&#x9;&#x9;&#x9; WHERE&#x9;VSRE_ID&#x9;&#x9;&#x9;&#x9;= EVAL_VSRE_ID+0&amp;#10;&#x9;&#x9;&#x9; AND&#x9;NVL(VSRE_NOT_RUNNING_IND,'N')&#x9;&lt;> 'Y'&amp;#10;&#x9;&#x9;&#x9; AND&#x9;VSRE_ACADEMIC_YEAR+0&#x9;&#x9;=&amp;#10;&#x9;&#x9;&#x9;&#x9; TO_NUMBER(TO_CHAR(ADD_MONTHS(:NROL.NROL_END_DATE,-8),'YYYY'))&amp;#10;&#x9;&#x9;&#x9; AND&#x9;VSRE_CERTIFICATION_IND&#x9;&#x9;IS NULL&amp;#10;&#x9;&#x9;&#x9; AND&#x9;VSRE_RATING&#x9;&#x9;&#x9;IS NULL&amp;#10;&#x9;&#x9;&#x9; AND&#x9;EVAL_NPP_ID&#x9;&#x9;&#x9;= :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;&#x9; AND&#x9;EVAL_CURRENT_IND&#x9;&#x9;= 'Y' );&amp;#10;&amp;#10;   &#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;           'WARNING...CURRENT INCOMPLETE QAS EV ALLOCATIONS EXIST');&amp;#10;  &#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;  &#x9;THEN NULL;&amp;#10;  &#x9;END IF;&amp;#10;&amp;#10;&#x9;EXCEPTION&amp;#10;&amp;#10;&#x9;&#x9;WHEN NO_DATA_FOUND THEN NULL;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_121" ProgramUnitText="PROCEDURE PR_VALIDATE_121 IS&amp;#10;BEGIN&amp;#10; &#x9;IF&#x9;NVL(:NROL.NROL_MAX_SCRIPTS,0)&#x9;> 0 &#x9;AND&amp;#10;&#x9;&#x9;:NROL.NROL_SERIES_ID IS NULL&#x9;&#x9;OR&amp;#10; &#x9;&#x9;NVL(:NROL.NROL_MAX_SCRIPTS_2,0)&#x9;> 0 &#x9;AND&amp;#10;&#x9;&#x9;:NROL.NROL_SERIES_ID_2 IS NULL&#x9;&#x9;OR&amp;#10; &#x9;&#x9;:NROL.NROL_MAX_SCRIPTS IS NULL &#x9;&#x9;AND&amp;#10;&#x9;&#x9;:NROL.NROL_SERIES_ID IS NOT NULL&#x9;OR&#x9;&#x9;&amp;#10; &#x9;&#x9;:NROL.NROL_MAX_SCRIPTS_2 IS NULL &#x9;AND&amp;#10;&#x9;&#x9;:NROL.NROL_SERIES_ID_2 IS NOT NULL&#x9;OR&amp;#10;&#x9;&#x9;NVL(:NROL.NROL_SERIES_ID,-1)&#x9;&#x9;=&amp;#10;&#x9;&#x9;NVL(:NROL.NROL_SERIES_ID_2,-2)&#x9;&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;          &#x9;'ERROR...test series / maximum scripts data inconsistent');&amp;#10;&#x9;  &#x9;IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10;  &#x9;&#x9;THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_FORMAT_NO_UNITS" ProgramUnitText="PROCEDURE PR_FORMAT_NO_UNITS IS&amp;#10;BEGIN&amp;#10;&amp;#10;  IF&#x9;:NROL.NROL_NRTY_ID = 130&amp;#10;  THEN&#x9;:NROL.NBT_NO_UNITS := TO_CHAR(:NROL.NROL_MAX_DAYS * 12)||' Units';&amp;#10;  ELSE&#x9;:NROL.NBT_NO_UNITS := NULL;&amp;#10;  END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_POSTCODE" ProgramUnitText="PROCEDURE PR_VALIDATE_POSTCODE IS&amp;#10;BEGIN&amp;#10;  &amp;#10;&#x9;IF&#x9;:NADD.NADD_ATYP_CODE = 'H' AND&amp;#10;&#x9;&#x9;:NADD.NADD_POSTCODE  IS NULL&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;MSG_ALERT('WARNING...NO HOME ADDRESS POSTCODE HAS BEEN ENTERED...'||&amp;#10;                          'AUTOMATIC VOCATIONAL ALLOCATIONS WILL NOT BE POSSIBLE...'||&amp;#10;                          'WWW.ROYALMAIL.CO.UK COULD BE OF USE !!','W',FALSE);&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_DUPE" ProgramUnitText="PROCEDURE PR_VALIDATE_DUPE IS&amp;#10;&amp;#10;&#x9;P_NPP_ID VARCHAR2(100);&amp;#10;&amp;#10;   &#x9;CURSOR C1 IS &#x9;SELECT &#x9;TO_CHAR(NPP_ID) NPP_ID&amp;#10;&#x9;&#x9; &#x9;FROM   &#x9;NPPS,&amp;#10;&#x9;&#x9;&#x9;&#x9;NPP_ADDRESSES&amp;#10;&#x9;&#x9; &#x9;WHERE  &#x9;NPP_DATE_OF_BIRTH &#x9;&#x9;IS NOT NULL&amp;#10;&#x9;&#x9; &#x9;AND    &#x9;NPP_DATE_OF_BIRTH &#x9;&#x9;= :NPP.NPP_DATE_OF_BIRTH&amp;#10;&#x9;&#x9;&#x9;AND&#x9;NPP_GENDER&#x9;&#x9;&#x9;IS NOT NULL&amp;#10;&#x9;&#x9;&#x9;AND&#x9;NPP_GENDER&#x9;&#x9;&#x9;= :NPP.NPP_GENDER&amp;#10;&#x9;&#x9;&#x9;AND&#x9;NPP_ID&#x9;&#x9;&#x9;&#x9;= NADD_NPP_ID+0&amp;#10;&#x9;&#x9;&#x9;AND&#x9;NADD_POSTCODE&#x9;&#x9;&#x9;= :NADD.NADD_POSTCODE&amp;#10;&#x9;&#x9;&#x9;AND&#x9;NADD_ATYP_CODE&#x9;&#x9;&#x9;= 'H'&amp;#10;&#x9;&#x9;&#x9;AND&#x9;SUBSTR(NADD_ADDRESS_LINE_1,1,2)&#x9;= SUBSTR(:NADD.NADD_ADDRESS_LINE_1,1,2);&amp;#10;&amp;#10;BEGIN&amp;#10;  &amp;#10;&#x9;IF&#x9;:NADD.NADD_ATYP_CODE &#x9;= 'H' AND&amp;#10;&#x9;&#x9;:NADD.NADD_POSTCODE  &#x9;IS NOT NULL AND&amp;#10;&#x9;&#x9;:NPP.NPP_DATE_OF_BIRTH&#x9;IS NOT NULL AND&amp;#10;&#x9;&#x9;:NPP.NPP_GENDER&#x9;&#x9;IS NOT NULL&amp;#10;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;P_NPP_ID := NULL;&amp;#10;&amp;#10;&#x9;&#x9;FOR R1 IN C1 LOOP&amp;#10;&amp;#10;&#x9;&#x9;&#x9;P_NPP_ID := P_NPP_ID||R1.NPP_ID||' ';&amp;#10;&amp;#10;&#x9;&#x9;END LOOP;&amp;#10; &amp;#10; &#x9;&#x9;IF &#x9;P_NPP_ID IS NOT NULL&amp;#10;&#x9; &#x9;THEN&amp;#10;&#x9;&#x9;&#x9;MSG_ALERT('ERROR...ONE OR MORE AA''S ALREADY EXIST WITH MATCHING HOME POSTCODE, DATE OF BIRTH,'||&amp;#10;        &#x9;         ' GENDER AND SIMILAR ADDRESS...'||P_NPP_ID,'E',TRUE);&amp;#10;&#x9;&#x9;&#x9;:NPP.NPP_ID := NULL;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_LINE1" ProgramUnitText="PROCEDURE PR_VALIDATE_LINE1 IS&amp;#10;BEGIN&amp;#10;&amp;#10;  &#x9;IF&#x9;:NADD.NADD_ADDRESS_LINE_1 IS NULL&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;MSG_ALERT('ERROR...1ST ADDRESS LINE MANDATORY','E',TRUE);&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NRTR" ProgramUnitText="PROCEDURE PR_VALIDATE_NRTR IS&amp;#10;BEGIN&amp;#10;  &amp;#10;  IF :NRTR.NRTR_DATE IS NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('DATE IS MANDATORY','E',TRUE);&amp;#10;  END IF;&amp;#10;  IF :NRTR.NRTR_YES_NO IS NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('TRAINING ATTENDANCE YES OR NO IS MANDATORY','E',TRUE);&amp;#10;  END IF;&amp;#10;  IF :NRTR.NRTR_YES_NO = 'Y' AND :NRTR.NRTR_REASON IS NOT NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('NON-ATTENDANCE REASON NOT ALLOWED','E',TRUE);&amp;#10;  END IF;&amp;#10;  IF :NRTR.NRTR_YES_NO = 'Y' AND :NRTR.NRTR_ACTION IS NOT NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('NON-ATTENDANCE ACTION NOT ALLOWED','E',TRUE);&amp;#10;  END IF;&amp;#10;  IF :NRTR.NRTR_YES_NO = 'N' AND :NRTR.NRTR_REASON IS NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('NON-ATTENDANCE REASON MANDATORY','E',TRUE);&amp;#10;  END IF;&amp;#10;  IF :NRTR.NRTR_YES_NO = 'N' AND :NRTR.NRTR_ACTION IS NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('NON-ATTENDANCE ACTION MANDATORY','E',TRUE);&amp;#10;  END IF;&amp;#10;  &amp;#10;  IF&#x9;:NRTR.NRTR_YES_NO = 'Y'&amp;#10;  THEN&amp;#10;  &amp;#10;   IF&#x9;:NRTR.NRTR_TYPE IS NULL&amp;#10;&#x9; THEN&amp;#10;&#x9;&#x9;MSG_ALERT('Training Type is Mandatory','E',TRUE);&amp;#10;&#x9; END IF;&amp;#10;&amp;#10;--&#x9; IF&#x9;:NROL.NROL_NRTY_ID = 110&amp;#10;--&#x9; THEN&amp;#10;--&#x9;&#x9;IF&#x9;:NRTR.NRTR_TYPE = 'LIVE'&amp;#10;--&#x9;&#x9;THEN&amp;#10;--&#x9;&#x9;&#x9;&#x9;IF&#x9;:NRTR.NRTR_SECTOR IS NULL&amp;#10;--&#x9;&#x9;&#x9;&#x9;THEN&amp;#10;--&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MSG_ALERT('NVQ Sector Must Be Input','E',TRUE);&amp;#10;--&#x9;&#x9;&#x9;&#x9;END IF;&amp;#10;--&#x9;&#x9;ELSE&amp;#10;--&#x9;&#x9;&#x9;&#x9;IF&#x9;:NRTR.NRTR_SECTOR IS NOT NULL&amp;#10;--&#x9;&#x9;&#x9;&#x9;THEN&amp;#10;--&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;MSG_ALERT('NVQ Sector Cannot Be Input','E',TRUE);&amp;#10;--&#x9;&#x9;&#x9;&#x9;END IF;&amp;#10;--&#x9;&#x9;END IF;&amp;#10;--&#x9; ELSE&amp;#10;--&#x9;&#x9;IF&#x9;:NRTR.NRTR_SECTOR IS NOT NULL&amp;#10;--&#x9;&#x9;THEN&#x9;&#x9;&#x9;&amp;#10;--&#x9;&#x9;&#x9;&#x9;MSG_ALERT('NVQ Sector Cannot Be Input','E',TRUE);&amp;#10;--&#x9;&#x9;END IF;&amp;#10;--&#x9; END IF;&amp;#10;&#x9; &amp;#10;&#x9;END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_VALIDATE_NRPE" ProgramUnitText="PROCEDURE PR_VALIDATE_NRPE IS&amp;#10;BEGIN&amp;#10;  &amp;#10;  IF :NRPE.NRPE_DATE IS NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('DATE IS MANDATORY','E',TRUE);&amp;#10;  END IF;&amp;#10;  IF :NRPE.NRPE_GRADE IS NULL&amp;#10;  THEN&amp;#10;  &#x9;&#x9;MSG_ALERT('GRADE IS MANDATORY','E',TRUE);&amp;#10;  END IF;&amp;#10;  &amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_MAX_CENTRES" ProgramUnitText="PROCEDURE PR_MAX_CENTRES IS&amp;#10;BEGIN&amp;#10;  &amp;#10;  SELECT&#x9;DECODE(NRPE_GRADE,&amp;#10;           'A',NRTY_A_MAX_CENTRES,&amp;#10;           'B',NRTY_B_MAX_CENTRES,&amp;#10;           'C',NRTY_C_MAX_CENTRES,&amp;#10;           'D',NRTY_D_MAX_CENTRES,&amp;#10;           'E',NRTY_E_MAX_CENTRES,0)&amp;#10;  INTO&#x9;&#x9;:NROL.NROL_MAX_CENTRES&amp;#10;  FROM&#x9;&#x9;NPP_ROLE_TYPES,&amp;#10;  &#x9;&#x9;&#x9;&#x9;NPP_ROLE_PERFORMANCE&amp;#10;  WHERE&#x9;&#x9;NRTY_ID &#x9;&#x9;&#x9;= :NROL.NROL_NRTY_ID&amp;#10;  AND&#x9;&#x9;&#x9;NRPE_NROL_ID &#x9;= :NROL.NROL_ID&amp;#10;  AND&#x9;&#x9;&#x9;NRPE_DATE     = ( SELECT MAX(NRPE_DATE)&amp;#10;                            FROM   NPP_ROLE_PERFORMANCE&amp;#10;                            WHERE  NRPE_NROL_ID = :NROL.NROL_ID );&amp;#10;      MSG_ALERT('MAXIMUM CENTRES HAS BEEN AUTOMATICALLY SET TO '||TO_CHAR(:NROL.NROL_MAX_CENTRES),'I',FALSE);   &amp;#10;                            &amp;#10;EXCEPTION&amp;#10;&#x9;&amp;#10;&#x9;WHEN NO_DATA_FOUND THEN :NROL.NROL_MAX_CENTRES := 0;&amp;#10;&#x9;MSG_ALERT('MAXIMUM CENTRES HAS BEEN AUTOMATICALLY SET TO 0','I',FALSE);&amp;#10;  &amp;#10;END;&amp;#10;&amp;#10;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_NRCO" ProgramUnitText="PROCEDURE PR_NRCO IS&amp;#10;BEGIN&amp;#10;  &amp;#10;  IF&#x9;:NRCO.NRCO_ACCEPT_IND     IS NULL AND&amp;#10;&#x9;  &#x9;:NRCO.NRCO_SQA_ACCEPT_IND IS NULL&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;MSG_ALERT('AT LEAST ONE ''ACCEPT? YES/NO'' IS MANDATORY...OTHERWISE DELETE RECORD','E',TRUE);&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;:NRCO.NRCO_ACCEPT_IND IS NOT NULL&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;IF&#x9;:NRCO.NRCO_DATE IS NULL&amp;#10;&#x9;&#x9;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_DATE := TRUNC(SYSDATE);&amp;#10;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;ELSE&amp;#10;&#x9;&#x9;:NRCO.NRCO_DATE := NULL;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;:NRCO.NRCO_SQA_ACCEPT_IND IS NOT NULL&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;IF&#x9;:NROL.NROL_NRTY_ID &lt;> 110&amp;#10;&#x9;&#x9;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;MSG_ALERT('ERROR...Only Valid for Role 110 NVQ','E',TRUE);&amp;#10;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;&#x9;IF&#x9;:NRCO.NRCO_SQA_DATE IS NULL&amp;#10;&#x9;&#x9;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_SQA_DATE := TRUNC(SYSDATE);&amp;#10;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;ELSE&amp;#10;&#x9;&#x9;&#x9;:NRCO.NRCO_SQA_DATE := NULL;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;:NROL.NROL_NRTY_ID IN (147,153)&amp;#10;&#x9;THEN&amp;#10;&amp;#10;  BEGIN&amp;#10;&amp;#10;&#x9;&#x9;INSERT INTO NPP_ROLE_CONTRACTS&amp;#10;&#x9;&#x9;( NRCO_NROL_ID,&amp;#10;&#x9;&#x9;  NRCO_YEAR,&amp;#10;&#x9;&#x9;  NRCO_DATE,&amp;#10;&#x9;&#x9;  NRCO_ACCEPT_IND,&amp;#10;&#x9;&#x9;  NRCO_INSERT_DATE,&amp;#10;&#x9;&#x9;  NRCO_INSERT_USER )&amp;#10;&#x9;&#x9;SELECT&#x9;NROL_ID,&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_YEAR,&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_DATE,&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_ACCEPT_IND,&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_INSERT_DATE,&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;:NRCO.NRCO_INSERT_USER&amp;#10;&#x9;&#x9;FROM&#x9;&#x9;NPP_ROLES&amp;#10;&#x9;&#x9;WHERE&#x9;&#x9;NROL_NPP_ID &#x9;&#x9;= :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;AND     NROL_NRTY_ID+0 &#x9;IN (147,153)&amp;#10;&#x9;&#x9;AND     NROL_NRTY_ID+0 &#x9;&lt;> :NROL.NROL_NRTY_ID&amp;#10;&#x9;&#x9;AND&#x9;&#x9;&#x9;NOT EXISTS ( SELECT NULL&amp;#10;&#x9;&#x9;                     FROM   NPP_ROLE_CONTRACTS&amp;#10;&#x9;&#x9;                     WHERE  NRCO_NROL_ID = NROL_ID&amp;#10;&#x9;&#x9;                     AND    NRCO_YEAR    = :NRCO.NRCO_YEAR );&amp;#10;&amp;#10;  END;&amp;#10;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;:NRCO.NRCO_ACCEPT_IND = 'N'&amp;#10;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;IF&#x9;:NROL.NROL_NRTY_ID = 551&amp;#10;&#x9;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;BEGIN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;&#x9;UPDATE &#x9;OSCA_OUTCOME_AAS&amp;#10;&#x9;&#x9;&#x9;&#x9;SET&#x9;&#x9;&#x9;OOAA_CURRENT_IND = 'N'&amp;#10;&#x9;&#x9;&#x9;&#x9;WHERE&#x9;&#x9;OOAA_CURRENT_IND = 'Y'&amp;#10;&#x9;&#x9;&#x9;&#x9;AND&#x9;&#x9;&#x9;OOAA_NPP_ID      = :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;AND     EXISTS ( SELECT NULL&amp;#10;&#x9;&#x9;&#x9;&#x9;                 FROM   APPROVAL_GROUPS,&amp;#10;&#x9;&#x9;&#x9;&#x9;                        VERIFICATION_SCHEDULE_RECORDS,&amp;#10;&#x9;&#x9;&#x9;&#x9;                        OSCA_OUTCOMES&amp;#10;&#x9;&#x9;&#x9;&#x9;                 WHERE  AGRO_CODE            = VSRE_ALLOCATION_CODE||''&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    AGRO_TYPE            IN ('FLT','WORKSKILLS')&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND&#x9;&#x9;VSRE_ID              = OOUT_VSRE_ID+0&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_ACADEMIC_YEAR+0 = :NRCO.NRCO_YEAR&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    OOUT_ID              = OOAA_OOUT_ID );&amp;#10;&amp;#10;&#x9;&#x9;&#x9;END;&amp;#10;&amp;#10;&#x9;&#x9;ELSE&amp;#10;&amp;#10;&#x9;&#x9;IF&#x9;:NROL.NROL_NRTY_ID = 114&amp;#10;&#x9;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;BEGIN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;&#x9;UPDATE &#x9;FEX_DETAILS&amp;#10;&#x9;&#x9;&#x9;&#x9;SET&#x9;&#x9;&#x9;FDET_CURRENT_IND = 'N'&amp;#10;&#x9;&#x9;&#x9;&#x9;WHERE&#x9;&#x9;FDET_CURRENT_IND = 'Y'&amp;#10;&#x9;&#x9;&#x9;&#x9;AND&#x9;&#x9;&#x9;FDET_NPP_ID      = :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;AND     EXISTS ( SELECT NULL&amp;#10;&#x9;&#x9;&#x9;&#x9;                 FROM   VERIFICATION_SCHEDULE_RECORDS&amp;#10;&#x9;&#x9;&#x9;&#x9;                 WHERE  VSRE_ID              = FDET_VSRE_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_ACADEMIC_YEAR+0 = :NRCO.NRCO_YEAR );&amp;#10;&amp;#10;&#x9;&#x9;&#x9;END;&amp;#10;&amp;#10;&#x9;&#x9;ELSE&amp;#10;&amp;#10;&#x9;&#x9;&#x9;BEGIN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;&#x9;UPDATE &#x9;EV_ALLOCATIONS&amp;#10;&#x9;&#x9;&#x9;&#x9;SET&#x9;&#x9;&#x9;EVAL_CURRENT_IND = 'N'&amp;#10;&#x9;&#x9;&#x9;&#x9;WHERE&#x9;&#x9;EVAL_CURRENT_IND = 'Y'&amp;#10;&#x9;&#x9;&#x9;&#x9;AND&#x9;&#x9;&#x9;EVAL_NPP_ID      = :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;AND     EXISTS ( SELECT NULL&amp;#10;&#x9;&#x9;&#x9;&#x9;                 FROM   CENTRES,&amp;#10;&#x9;&#x9;&#x9;&#x9;                        VERIFICATION_SCHEDULE_RECORDS&amp;#10;&#x9;&#x9;&#x9;&#x9;                 WHERE  CN_CENTRE_ID         = VSRE_CENTRE_ID||''&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    CN_BURE_BUNI_ID+0    > 0&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_ID              = EVAL_VSRE_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND  ((:NROL.NROL_NRTY_ID   = 1&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSME_CODE       IN ('INTRO','EMOD'))&amp;#10;&#x9;&#x9;&#x9;&#x9;                 OR    (:NROL.NROL_NRTY_ID   = 200&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSTY_CODE       IN ('BTECP')&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSME_CODE       IN ('AUDIT'))&amp;#10;&#x9;&#x9;&#x9;&#x9;                 OR    (:NROL.NROL_NRTY_ID   = 106&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSTY_CODE       IN ('BTECP','BTECU')&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSME_CODE       IN ('VISIT'))&amp;#10;&#x9;&#x9;&#x9;&#x9;                 OR    (:NROL.NROL_NRTY_ID   = 110&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    CN_BURE_BUNI_ID&#x9;&#x9;&#x9; &lt;> 6&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSTY_CODE       IN ('NVQP','NVQU'))&amp;#10;&#x9;&#x9;&#x9;&#x9;                 OR    (:NROL.NROL_NRTY_ID   IN (147,152,153)&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSME_CODE       = 'NSS')&amp;#10;&#x9;&#x9;&#x9;&#x9;                 OR    (:NROL.NROL_NRTY_ID   = 165&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSME_CODE       = 'EEX'))&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND&#x9;&#x9;VSRE_ACADEMIC_YEAR+0 > 2004&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_ACADEMIC_YEAR+0 = :NRCO.NRCO_YEAR );&amp;#10;&amp;#10;&#x9;&#x9;&#x9;END;&amp;#10;&amp;#10;&#x9;&#x9;&#x9;if   :nrol.nrol_nrty_id = 200&amp;#10;&#x9;&#x9;&#x9;then&amp;#10;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;&#x9;&#x9;begin&amp;#10;&#x9;&#x9;&#x9;&#x9;&amp;#10;    &#x9;&#x9;&#x9;update  approval_progress&amp;#10;    &#x9;&#x9;&#x9;set     ap_ext_brief_person_code = null&amp;#10;    &#x9;&#x9;&#x9;where   ap_ext_brief_person_code = ltrim(to_char(:nrol.nrol_npp_id))&amp;#10;    &#x9;&#x9;&#x9;and     ap_val_method_code       = '6'&amp;#10;    &#x9;&#x9;&#x9;and     ap_rejected              is null&amp;#10;    &#x9;&#x9;&#x9;and     ap_approved              is null&amp;#10;    &#x9;&#x9;&#x9;and     ap_withdrawn             is null&amp;#10;    &#x9;&#x9;&#x9;and     ap_briefer_decision      is null;&amp;#10;    &#x9;&#x9;&#x9;&amp;#10;    &#x9;&#x9;end;&amp;#10;    &amp;#10;&#x9;&#x9;&#x9;end if;&amp;#10;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&amp;#10;&#x9;&#x9;MSG_ALERT('ANY CURRENT ALLOCATIONS FOR THE SPECIFIED YEAR HAVE BEEN REMOVED','I',FALSE);&amp;#10;&#x9;&#x9;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;&#x9;IF&#x9;:NRCO.NRCO_SQA_ACCEPT_IND = 'N'&amp;#10;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;IF&#x9;:NROL.NROL_NRTY_ID = 110&amp;#10;&#x9;&#x9;THEN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;BEGIN&amp;#10;&amp;#10;&#x9;&#x9;&#x9;&#x9;UPDATE &#x9;EV_ALLOCATIONS&amp;#10;&#x9;&#x9;&#x9;&#x9;SET&#x9;&#x9;&#x9;EVAL_CURRENT_IND = 'N'&amp;#10;&#x9;&#x9;&#x9;&#x9;WHERE&#x9;&#x9;EVAL_CURRENT_IND = 'Y'&amp;#10;&#x9;&#x9;&#x9;&#x9;AND&#x9;&#x9;&#x9;EVAL_NPP_ID      = :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;AND     EXISTS ( SELECT NULL&amp;#10;&#x9;&#x9;&#x9;&#x9;                 FROM   CENTRES,&amp;#10;&#x9;&#x9;&#x9;&#x9;                        VERIFICATION_SCHEDULE_RECORDS&amp;#10;&#x9;&#x9;&#x9;&#x9;                 WHERE  CN_CENTRE_ID         = VSRE_CENTRE_ID||''&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_ID              = EVAL_VSRE_ID&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    CN_BURE_BUNI_ID+0&#x9;&#x9; = 6&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_VSTY_CODE       IN ('NVQP','NVQU')&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND&#x9;&#x9;VSRE_ACADEMIC_YEAR+0 > 2004&amp;#10;&#x9;&#x9;&#x9;&#x9;                 AND    VSRE_ACADEMIC_YEAR+0 = :NRCO.NRCO_YEAR );&amp;#10;&amp;#10;&#x9;&#x9;&#x9;END;&amp;#10;&amp;#10;&#x9;&#x9;&#x9;MSG_ALERT('ANY CURRENT NVQ AWARDS UK ALLOCATIONS FOR THE SPECIFIED YEAR HAVE BEEN REMOVED','I',FALSE);&amp;#10;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;&amp;#10;&#x9;END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="PR_CALCULATE_QCOD_CODES" ProgramUnitText="PROCEDURE pr_calculate_qcod_codes IS&amp;#10;BEGIN&amp;#10;&amp;#10;:NROL.NBT_QCOD_CODE_D32 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_D33 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_D34 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_D35 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_A1 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_A2 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_V1 := NULL;&amp;#10;:NROL.NBT_QCOD_CODE_V2 := NULL;&amp;#10;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_D32 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_D33 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_D34 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_D35 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_A1 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_A2 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_V1 := NULL;&amp;#10;:NROL.NBT_NQUA_QCOD_ID_V2 := NULL;&amp;#10;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_D32 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_D33 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_D34 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_D35 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_A1 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_A2 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_V1 := NULL;&amp;#10;:NROL.NBT_QCOD_ID_UPDATE_V2 := NULL;&amp;#10;&amp;#10;&amp;#10; IF :NROL.NROL_NRTY_ID &lt;> 110 THEN&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D32',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D33',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D34',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D35',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_A1',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_A2',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_V1',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_V2',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_V2_START',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_V2_FINISH',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  &amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_V2_AWARDING_BODY',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_V2_CERTIFICATE_NO',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  &amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_D35_AWARDING_BODY',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_D35_CERTIFICATE_NO',VISIBLE,PROPERTY_FALSE);&amp;#10; &#x9;  &amp;#10; ELSIF :NROL.NROL_NRTY_ID = 110 THEN&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D32',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D33',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D34',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D35',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_A1',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_A2',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_V1',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_V2',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  &amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_V2_AWARDING_BODY',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_V2_CERTIFICATE_NO',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  &amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_D35_AWARDING_BODY',VISIBLE,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NROL_D35_CERTIFICATE_NO',VISIBLE,PROPERTY_TRUE);&amp;#10; &amp;#10; &#x9; &amp;#10;    SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D32',ENABLED,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D33',ENABLED,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D34',ENABLED,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_D35',ENABLED,PROPERTY_TRUE);&amp;#10; &#x9;  SET_ITEM_PROPERTY('NROL.NBT_QCOD_CODE_A1',ENABLED,PROPERTY