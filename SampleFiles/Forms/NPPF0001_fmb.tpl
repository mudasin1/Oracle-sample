      <ProgramUnit Name="PR_COUNTRY" ProgramUnitText="PROCEDURE PR_COUNTRY IS&amp;#10;&amp;#10;  C_MESSAGE VARCHAR2(2);&amp;#10;  C_COUNTRY VARCHAR2(2);&amp;#10;  C_RETURN  VARCHAR2(10);&amp;#10;&amp;#10;BEGIN&amp;#10;  &amp;#10;  C_MESSAGE := NULL;&amp;#10;  C_COUNTRY := NULL;&amp;#10;  C_RETURN  := NULL;               &amp;#10;                  &amp;#10;  C_RETURN :=  fn_nadd_derive_ccode(2,0,:NADD.NADD_TOWN,:NADD.NADD_COUNTY,:NADD.NADD_POSTCODE,&amp;#10;                                        :NADD.NADD_GRID_REFERENCE_EAST,:NADD.NADD_GRID_REFERENCE_NORTH);&amp;#10;                 &amp;#10;--  MSG_ALERT('C_RETURN = '||C_RETURN,'I',FALSE);&amp;#10;  &amp;#10; &#x9;SELECT  SUBSTR(C_RETURN,1,INSTR(C_RETURN,':')-1),&amp;#10; &#x9;&#x9;&#x9;&#x9;&#x9;SUBSTR(C_RETURN,INSTR(C_RETURN,':')+1)&amp;#10; &#x9;INTO&#x9;&#x9;C_MESSAGE,&amp;#10; &#x9;&#x9;&#x9;&#x9;&#x9;C_COUNTRY&amp;#10;&#x9;FROM    DUAL;&amp;#10;&amp;#10;--  MSG_ALERT('C_MESSAGE = '||C_MESSAGE,'I',FALSE);&amp;#10;--  MSG_ALERT('C_COUNTRY = '||C_COUNTRY,'I',FALSE);&amp;#10;&amp;#10;&#x9;IF&#x9;C_COUNTRY IS NOT NULL&amp;#10;  THEN&amp;#10;  &amp;#10;    :NADD.NADD_CIA2_ID := C_COUNTRY;&amp;#10;  &amp;#10;    IF&#x9;&#x9;C_MESSAGE = '0'&amp;#10;    THEN&amp;#10;    &#x9;&#x9;&#x9;MSG_ALERT('WARNING...country could not be found and has been defaulted to GB','W',FALSE);&amp;#10;    ELSE&amp;#10;    &#x9;&#x9;&#x9;MSG_ALERT('INFO...country has been defaulted...please check','I',FALSE);&amp;#10;    END IF;&amp;#10;  &amp;#10;  &#x9;BEGIN&amp;#10;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;SELECT  CIA2_NAME&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;INTO&#x9;&#x9;:NADD.NBT_COUNTRY_NAME&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;FROM    COUNTRIES_ISO_ALPHA2&amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE   CIA2_ID = C_COUNTRY;&amp;#10;&#x9;&amp;#10;  &#x9;&#x9;&#x9;&#x9;EXCEPTION&amp;#10; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN NO_DATA_FOUND THEN NULL;&amp;#10;  &amp;#10;&#x9;&#x9;END;&amp;#10;&amp;#10;  END IF;&amp;#10;&amp;#10;END;" ProgramUnitType="Procedure"/>
      <ProgramUnit Name="FN_NPPS_REQUIRE_CLEANUP" ProgramUnitText="FUNCTION fn_npps_require_cleanup (p_in_npp_field       in varchar2,&amp;#10;                                  p_in_npp_field_value in varchar2,&amp;#10;                                  p_in_mandatory       in varchar2,&amp;#10;                                  p_in_length          in number)&amp;#10;RETURN varchar2 &amp;#10;IS&amp;#10;  cursor c1 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where  not REGEXP_LIKE (p_output_string,'^[A-Za-z ''&amp;-]+$');&amp;#10;&amp;#10;          cursor c2 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where regexp_like(p_output_string,'([[:space:]]{2,})');         &amp;#10;&amp;#10;          cursor c3 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where  not REGEXP_LIKE (p_output_string,'^[A-Za-z0-9 ''&amp;-]+$');&amp;#10;&amp;#10;          cursor c4 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where not regexp_like(p_output_string,'[0-9]{2}-[A-Za-z]{3}-[0-9]{2}');&amp;#10;&amp;#10;          cursor c5 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where  not REGEXP_LIKE (p_output_string,'[A-Z]{2}+[0-9]{6}+[A-Z]{1}');&amp;#10;      &amp;#10;          cursor c6 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where  not REGEXP_LIKE (p_output_string,'[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}', 'i');&amp;#10;&amp;#10;          cursor c7 (p_output_string varchar2)&amp;#10;           is &amp;#10;           SELECT 'Y'&amp;#10;           FROM COUNTRIES_ISO_ALPHA3&amp;#10;           WHERE INSTR(p_output_string,CIAL_NAME) = 0&amp;#10;           AND  INSTR(CIAL_NAME,'ENGLAND') = 0&amp;#10;           AND  INSTR(CIAL_NAME,'NORTHERN IRELAND') = 0&amp;#10;           AND  INSTR(CIAL_NAME,'SCOTLAND') = 0&amp;#10;           AND  INSTR(CIAL_NAME,'WALES') = 0&amp;#10;           AND  CIAL_UK_IND IS NULL&amp;#10;           AND  INSTR(p_output_string,'ENGLAND') = 0&amp;#10;           AND  INSTR(p_output_string,'NORTHERN IRELAND') = 0&amp;#10;           AND  INSTR(p_output_string,'SCOTLAND') = 0&amp;#10;           AND  INSTR(p_output_string,'WALES') = 0;&amp;#10;&amp;#10;          cursor c8 (p_output_string varchar2)&amp;#10;          is&amp;#10;           SELECT 'Y'&amp;#10;           FROM DUAL&amp;#10;           WHERE NVL(LENGTH(P_OUTPUT_STRING), 0) > 5&amp;#10;           AND SUBSTR(P_OUTPUT_STRING,1,1)   BETWEEN 'A' AND 'Z'&amp;#10;           AND  (SUBSTR(P_OUTPUT_STRING,2,1) BETWEEN '0' AND '9'&amp;#10;               OR &amp;#10;                 SUBSTR(P_OUTPUT_STRING,3,1)  BETWEEN '0' AND '9')&amp;#10;            AND  SUBSTR(P_OUTPUT_STRING, INSTR(P_OUTPUT_STRING,' ')+1,1) BETWEEN '0' AND '9'&amp;#10;            AND  SUBSTR(P_OUTPUT_STRING, INSTR(P_OUTPUT_STRING,' ')+2,1) BETWEEN 'A' AND 'Z'&amp;#10;            AND  SUBSTR(P_OUTPUT_STRING, INSTR(P_OUTPUT_STRING,' ')+3,1) BETWEEN 'A' AND 'Z'&amp;#10;            AND  SUBSTR(P_OUTPUT_STRING||'*', INSTR(P_OUTPUT_STRING,' ')+4,1) = '*'&amp;#10;             UNION&amp;#10;             SELECT 'Y'&amp;#10;             FROM DUAL&amp;#10;             WHERE NVL(LENGTH(P_OUTPUT_STRING), 0) > 5&amp;#10;             AND SUBSTR(P_OUTPUT_STRING,1,1)      BETWEEN 'A' AND 'Z'&amp;#10;             AND  (SUBSTR(P_OUTPUT_STRING,2,1) BETWEEN '0' AND '9'&amp;#10;               OR  SUBSTR(P_OUTPUT_STRING,3,1)  BETWEEN '0' AND '9')&amp;#10;             AND  SUBSTR(P_OUTPUT_STRING, INSTR(P_OUTPUT_STRING,' ')+1,1) BETWEEN '0' AND '9'&amp;#10;             AND  SUBSTR(P_OUTPUT_STRING, INSTR(P_OUTPUT_STRING,' ')+2,1) BETWEEN 'A' AND 'Z'&amp;#10;             AND  SUBSTR(P_OUTPUT_STRING, INSTR(P_OUTPUT_STRING,' ')+3,1) BETWEEN 'A' AND 'Z'&amp;#10;             AND  SUBSTR(P_OUTPUT_STRING||'*', INSTR(P_OUTPUT_STRING,' ')+4,1) =  '*';&amp;#10;&amp;#10;             cursor c9 (p_output_string varchar2)&amp;#10;             is&amp;#10;              SELECT 'Y'&amp;#10;              FROM COUNTRIES_ISO_ALPHA3&amp;#10;              WHERE INSTR(P_OUTPUT_STRING,CIAL_NAME) &lt;> 0&amp;#10;              AND  INSTR(CIAL_NAME,'ENGLAND') = 0&amp;#10;              AND  INSTR(CIAL_NAME,'NORTHERN IRELAND') = 0&amp;#10;              AND  INSTR(CIAL_NAME,'SCOTLAND') = 0&amp;#10;              AND  INSTR(CIAL_NAME,'WALES') = 0&amp;#10;              AND   CIAL_UK_IND IS NULL&amp;#10;              AND  INSTR(P_OUTPUT_STRING,'ENGLAND') = 0&amp;#10;              AND  INSTR(P_OUTPUT_STRING,'NORTHERN IRELAND') = 0&amp;#10;              AND  INSTR(P_OUTPUT_STRING,'SCOTLAND') = 0&amp;#10;              AND  INSTR(P_OUTPUT_STRING,'WALES') = 0;&amp;#10;&amp;#10;          cursor c10 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;          where  not REGEXP_LIKE (p_output_string,'^[A-Za-z ]+$');&amp;#10;&amp;#10;          cursor c11 (p_output_string varchar2)&amp;#10;          is&amp;#10;          select 'Y'&amp;#10;          from dual&amp;#10;                    where not regexp_like(p_output_string,'[0-9]{6}');&amp;#10;&amp;#10;v_rule_1 varchar2(1) := 'N';&amp;#10;v_rule_2 varchar2(1) := 'N';&amp;#10;v_rule_3 varchar2(1) := 'N';&amp;#10;v_rule_4 varchar2(1) := 'N'; -- Title&amp;#10;v_rule_5 varchar2(1) := 'N'; -- Gender&amp;#10;v_rule_6 varchar2(1) := 'N'; -- DOB&amp;#10;v_rule_7 varchar2(1) := 'N'; -- NI Number&amp;#10;v_rule_8 varchar2(1) := 'N'; -- Address1,2,3,Town&amp;#10;v_rule_9 varchar2(1) := 'N'; -- Email&amp;#10;v_rule_10 varchar2(1) := 'N'; -- Country&amp;#10;v_rule_11 varchar2(1) := 'N'; -- Postcode&amp;#10;v_rule_12 varchar2(1) := 'N'; -- Country Code&amp;#10;v_rule_13 varchar2(1) := 'N'; -- County&amp;#10;v_rule_14 varchar2(1) := 'N'; -- NPP Id&amp;#10;&amp;#10;v_evaluation_string varchar2(500) := null;&amp;#10;n_string_length number := 0;&amp;#10;v_output_string varchar2(1) := null;&amp;#10;v_npp_field varchar2(200) := null;&amp;#10;&amp;#10;begin&amp;#10;&amp;#10;    v_rule_1 := 'N';&amp;#10;    v_rule_2 := 'N';&amp;#10;    v_rule_3 := 'N';&amp;#10;    v_rule_4 := 'N';&amp;#10;    v_rule_5 := 'N';&amp;#10;    v_rule_6 := 'N';&amp;#10;    v_rule_7 := 'N';&amp;#10;    v_rule_8 := 'N';&amp;#10;    v_rule_9 := 'N';&amp;#10;    v_rule_10 := 'N';&amp;#10;    v_rule_11 := 'N';&amp;#10;    v_rule_12 := 'N';&amp;#10;    v_rule_13 := 'N';&amp;#10;    v_rule_14 := 'N';&amp;#10;    &amp;#10;    n_string_length := 0;&amp;#10;    v_evaluation_string := null;&amp;#10;&amp;#10;      v_npp_field         := UPPER(p_in_npp_field);&amp;#10;      v_evaluation_string := UPPER(p_in_npp_field_value);&amp;#10;      n_string_length     := length(p_in_npp_field_value);&amp;#10;      &amp;#10;     if n_string_length > p_in_length then&amp;#10;        v_rule_1 :=  'Y';&amp;#10;     elsif v_evaluation_string is null and p_in_mandatory = 'Y'&amp;#10;     then&amp;#10;        v_rule_2 :=  'Y';&amp;#10;     elsif v_evaluation_string is not null and &amp;#10;           v_npp_field = 'NPP_TITLE' and&amp;#10;           v_evaluation_string not in ('MR','MISS','MRS','MS','DR')&amp;#10;     then&amp;#10;        v_rule_4 := 'Y';       &amp;#10;     elsif v_evaluation_string is not null and &amp;#10;           v_npp_field = 'NPP_GENDER' and&amp;#10;                v_evaluation_string not in ('M','F')&amp;#10;     then&amp;#10;        v_rule_5 := 'Y';       &amp;#10;     elsif v_evaluation_string is not null and &amp;#10;           v_npp_field = 'NPP_DATE_OF_BIRTH'&amp;#10;     then&amp;#10;       open c4 (v_evaluation_string);&amp;#10;       fetch c4 into v_rule_6;&amp;#10;       close c4;&amp;#10;       if v_rule_6 is null then&amp;#10;          v_rule_6 := 'N';&amp;#10;       end if;&amp;#10;     elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;           v_npp_field = 'NPP_NI_NUMBER' &amp;#10;     then&amp;#10;           if substr(v_evaluation_string,1,2) = 'TN'&amp;#10;           then&amp;#10;              v_rule_7 := 'Y';&amp;#10;           else&amp;#10;             open c5 (v_evaluation_string);&amp;#10;             fetch c5 into v_rule_7;&amp;#10;             close c5;&amp;#10;             if v_rule_7 is null then&amp;#10;                v_rule_7 := 'N';&amp;#10;             end if;&amp;#10;           end if;&amp;#10;     elsif  n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;            v_npp_field in ('NADD_ADDRESS_LINE_1','NADD_ADDRESS_LINE_2','NADD_ADDRESS_LINE_3','NADD_TOWN')&amp;#10;     then      &amp;#10;         open c2(v_evaluation_string);&amp;#10;         fetch c2 into v_rule_8;&amp;#10;         close c2;&amp;#10;&amp;#10;         if v_rule_8 is null then&amp;#10;            v_rule_8 := 'N';&amp;#10;         end if;&amp;#10;&amp;#10;        v_output_string := null;&amp;#10;        &amp;#10;      for i in 1..n_string_length loop&amp;#10;      &#x9;&amp;#10;          v_output_string := substr(v_evaluation_string,i,1);&amp;#10;&amp;#10;          for r3 in c3(v_output_string)&amp;#10;          loop&amp;#10;             v_rule_8 := 'Y';&amp;#10;          end loop;&amp;#10;            &amp;#10;      end loop;         &amp;#10;     elsif  n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;            v_npp_field = 'NADD_COUNTY'&amp;#10;     then      &amp;#10;         open c2(v_evaluation_string);&amp;#10;         fetch c2 into v_rule_13;&amp;#10;         close c2;&amp;#10;&amp;#10;         if v_rule_13 is null then&amp;#10;            v_rule_13 := 'N';&amp;#10;         end if;&amp;#10;&amp;#10;        v_output_string := null;&amp;#10;        &amp;#10;        for i in 1..n_string_length loop&amp;#10;&amp;#10;          v_output_string := substr(v_evaluation_string,i,1);&amp;#10;&amp;#10;          for r10 in c10(v_output_string)&amp;#10;          loop&amp;#10;             v_rule_13 := 'Y';&amp;#10;          end loop;&amp;#10;            &amp;#10;      end loop;            &amp;#10;     elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;          v_npp_field = 'NPP_ID'&amp;#10;     then&amp;#10;           open c11(v_evaluation_string);&amp;#10;            fetch c11 into v_rule_14;&amp;#10;            close c11;&amp;#10;            if v_rule_14 is null then&amp;#10;               v_rule_14 := 'N';&amp;#10;            end if; &amp;#10;            elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;            &#x9;  v_npp_field  = 'NADD_EMAIL_ADDRESS'&amp;#10;     then&amp;#10;           open c6(v_evaluation_string);&amp;#10;         fetch c6 into v_rule_9;&amp;#10;         close c6;&amp;#10;&amp;#10;         if v_rule_9 is null then&amp;#10;            v_rule_9 := 'N';&amp;#10;         end if;    &amp;#10;      elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;           v_npp_field  = 'COUNTRY'&amp;#10;     then&amp;#10;           open c7(v_evaluation_string);&amp;#10;           fetch c7 into v_rule_10;&amp;#10;           close c7;&amp;#10;&amp;#10;         if v_rule_10 is null then&amp;#10;            v_rule_10 := 'N';&amp;#10;         end if; &amp;#10;         elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;         &#x9;  v_npp_field = 'NADD_POSTCODE'&amp;#10;     then&amp;#10;            open c8(v_evaluation_string);&amp;#10;            fetch c8 into v_rule_11;&amp;#10;            close c8;&amp;#10;            if v_rule_11 is null then&amp;#10;               v_rule_11 := 'Y';&amp;#10;            else&amp;#10;               v_rule_11 := 'N';&amp;#10;            end if; &amp;#10;     elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and &amp;#10;           v_npp_field = 'COUNTRY_CODE'&amp;#10;     then&amp;#10;          open c9(v_evaluation_string);&amp;#10;          fetch c9 into v_rule_12;&amp;#10;          close c9;&amp;#10;          if v_rule_12 is null then&amp;#10;             v_rule_12 := 'N';&amp;#10;          end if;    &amp;#10;          elsif n_string_length &lt;= p_in_length and v_evaluation_string is not null and&amp;#10;          &#x9;    v_npp_field in ('NPP_SURNAME','NPP_FORENAMES')&amp;#10;     then &amp;#10;         open c2(v_evaluation_string);&amp;#10;         fetch c2 into v_rule_3;&amp;#10;         close c2;&amp;#10;&amp;#10;         if v_rule_3 is null then&amp;#10;            v_rule_3 := 'N';&amp;#10;         end if;&amp;#10;&amp;#10;        v_output_string := null;&amp;#10;        &amp;#10;      for i in 1..n_string_length loop&amp;#10;&amp;#10;          v_output_string := substr(v_evaluation_string,i,1);&amp;#10;&amp;#10;          for r1 in c1(v_output_string)&amp;#10;          loop&amp;#10;             v_rule_3 := 'Y';&amp;#10;          end loop;&amp;#10;            end loop;          &amp;#10;   &amp;#10;    end if;&amp;#10;&amp;#10;         if v_rule_1 = 'Y' or&amp;#10;            v_rule_2 = 'Y' or&amp;#10;            v_rule_3 = 'Y' or&amp;#10;            v_rule_4 = 'Y' or&amp;#10;            v_rule_5 = 'Y' or&amp;#10;           v_rule_6 = 'Y' or&amp;#10;           v_rule_7 = 'Y' or&amp;#10;            v_rule_8 = 'Y' or&amp;#10;            v_rule_9 = 'Y' or&amp;#10;            v_rule_10 = 'Y' or&amp;#10;            v_rule_11 = 'Y' or&amp;#10;            v_rule_12 = 'Y' or&amp;#10;            v_rule_13 = 'Y' or&amp;#10;            v_rule_14 = 'Y'&amp;#10;        then&amp;#10;            return 'Y';&amp;#10;                  else &amp;#10;            return 'N';&amp;#10;        end if;&amp;#10;end;&amp;#10; &amp;#10;" ProgramUnitType="Function"/>
      <ProgramUnit Name="PR_RECALCULATE_PAYGROUP" ProgramUnitText="PROCEDURE PR_RECALCULATE_PAYGROUP IS&amp;#10;BEGIN&amp;#10;&#x9;--EC2103&amp;#10;&#x9;--:NPP.NBT_NPAY_ID_AUTO := 'Y';&amp;#10;&#x9;&amp;#10;  --wi1358&amp;#10;DECLARE&amp;#10;&#x9;CURSOR c_old_nadd IS&amp;#10;&#x9;  SELECT NADD_CIA2_ID&amp;#10;&#x9;  FROM  NPP_ADDRESSES&amp;#10;&#x9;  WHERE NADD_NPP_ID = :NPP.NPP_ID&amp;#10;&#x9;  AND   NADD_ATYP_CODE = 'H'&amp;#10;&#x9;  AND   NADD_CIA2_ID = :NPP.COPY_NPP_CIA2_ID;&amp;#10;&#x9;  &amp;#10;&#x9;v_old_nadd_cia2_id npp_addresses.nadd_cia2_id%type := null;&amp;#10;&#x9;&amp;#10;BEGIN&amp;#10;&amp;#10;/*&amp;#10;PayGroup&#x9;Paygroup in IQS&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Paygroup in ADP&#x9;&#x9;Country&#x9;&#x9;Q1 (Q3)&#x9;&#x9;Q2 (Q4)&#x9;&#x9;Q3 (Q5)&#x9;&#x9;Q4 (Q6)&#x9;&#x9;Q5 (Q7)&#x9;&#x9;Q6 (Q8) &amp;#10;==================================================================================================================================&amp;#10;1&#x9;&#x9;&#x9;&#x9;&#x9;UK RESIDENT&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&lt;ADP to assign>&#x9;&#x9;&#x9;GB&#x9;&#x9;&#x9;Those with a GB country code to be automatically assigned to PG1&#x9;&amp;#10;2&#x9;&#x9;&#x9;&#x9;&#x9;FC-NON TAXED SUPPLIERS&#x9;&#x9;&#x9;&#x9;FC_SUPP&#x9;&#x9;&#x9;&#x9;&#x9;Non - UK&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;Non GBP&#x9;&#x9;&#x9;N&amp;#10;3&#x9;&#x9;&#x9;&#x9;&#x9;SUPPLIERS&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SUPPLIERS&#x9;&#x9;&#x9;&#x9;Non - UK&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;&#x9;GBP&#x9;&#x9;&#x9;&#x9;N&amp;#10;4&#x9;&#x9;&#x9;&#x9;&#x9;UK TAXED GBP&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INTER2&#x9;&#x9;&#x9;&#x9;Non - UK&#x9;&#x9;*&#x9;&#x9;&#x9;&#x9;&#x9;*&#x9;&#x9;&#x9;&#x9;&#x9;*&#x9;&#x9;&#x9;&#x9;Y or N&#x9;&#x9;&#x9;GBP&#x9;&#x9;&#x9;&#x9;*&amp;#10;5&#x9;&#x9;&#x9;&#x9;&#x9;UK TAXED FOREIGN CURRENCY&#x9;&#x9;&#x9;&#x9;INTER&#x9;&#x9;&#x9;&#x9;&#x9;Non - UK&#x9;&#x9;*&#x9;&#x9;&#x9;&#x9;&#x9;*&#x9;&#x9;&#x9;&#x9;&#x9;*&#x9;&#x9;&#x9;&#x9;&#x9;N&#x9;&#x9;&#x9;&#x9;Non GBP&#x9;&#x9;&#x9;*&amp;#10;&amp;#10;* = if any one response is a Yes&amp;#10;&amp;#10;-- Above is the logic provided by Zahra Taghizadeh in  INC5789714&amp;#10;*/&amp;#10;&amp;#10;&amp;#10;&#x9;v_old_nadd_cia2_id := null;&amp;#10;&#x9;&amp;#10;&#x9;OPEN c_old_nadd;&amp;#10;&#x9;FETCH c_old_nadd INTO v_old_nadd_cia2_id;&amp;#10;&#x9;CLOSE c_old_nadd;&amp;#10;&amp;#10;-- EC4982 Added the below IF condition for the new Paygroup recalculation logic&amp;#10;   &amp;#10;IF&#x9;:NADD.NADD_CIA2_ID &lt;> 'GB' AND  &#x9;&amp;#10;&#x9;&#x9;:NADD.NADD_ATYP_CODE = 'H' AND&amp;#10;&#x9;&#x9;(:NPP.NPP_HMRC_INT_Q6 IS NULL OR :NPP.NPP_HMRC_INT_Q7 IS NULL)  &amp;#10;&amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9; SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9; 'WARNING !! Q4 and Q5 should not be blank ...'||&amp;#10;                 '...Please change accordingly .');&amp;#10;&amp;#10;&#x9;&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&amp;#10;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;  &#x9;THEN &amp;#10;  &#x9;&#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;END IF;&amp;#10;END IF;&amp;#10;&#x9;&amp;#10;&#x9; &amp;#10;&#x9;&amp;#10;-- Paygroup 1 (UK RESIDENT)  &amp;#10;IF :NADD.NADD_CIA2_ID = 'GB'  AND  :NADD.NADD_ATYP_CODE = 'H' &amp;#10;THEN&amp;#10;-- EC4982 Commented the below logic and added the assigning of Paygroup 1&amp;#10;&#x9;&#x9;      :NPP.NPP_NPAY_ID := 1;&amp;#10;   --EC1922&amp;#10;/*   IF :NPP.COPY_NPP_NPAY_ID IS NOT NULL &amp;#10;   THEN&amp;#10;    IF :NPP.COPY_NPP_NPAY_ID IN (2,3,4,5) &amp;#10;    THEN&amp;#10;    &#x9; IF v_old_nadd_cia2_id &lt;> 'GB' THEN&amp;#10;    &#x9; &#x9;  &amp;#10;&#x9;       :NPP.NPP_HMRC_INT_Q3 := NULL;&amp;#10;&#x9;       :NPP.NPP_HMRC_INT_Q4 := NULL;&amp;#10;&#x9;       :NPP.NPP_HMRC_INT_Q5 := NULL;&amp;#10;&#x9;       :NPP.NPP_HMRC_INT_Q6 := NULL;&amp;#10;&#x9;       :NPP.NPP_HMRC_INT_Q7 := NULL;&amp;#10;&#x9;       :NPP.NPP_HMRC_INT_Q8 := NULL;&amp;#10;&#x9;       &amp;#10;    &#x9; &#x9; :NPP.NPP_NPAY_ID := 1;&amp;#10;   &#x9;&#x9;&#x9;ELSE&amp;#10;&#x9;&#x9;    &#x9;:NPP.NPP_HMRC_INT_Q3 := NULL;&amp;#10;&#x9;&#x9;&#x9;    :NPP.NPP_HMRC_INT_Q4 := NULL;&amp;#10;&#x9;&#x9;&#x9;    :NPP.NPP_HMRC_INT_Q5 := NULL;&amp;#10;&#x9;&#x9;&#x9;    :NPP.NPP_HMRC_INT_Q6 := NULL;&amp;#10;&#x9;&#x9;&#x9;    :NPP.NPP_HMRC_INT_Q7 := NULL;&amp;#10;&#x9;&#x9;&#x9;    :NPP.NPP_HMRC_INT_Q8 := NULL;&amp;#10;&#x9;&#x9;&#x9;       &amp;#10;&#x9;&#x9;      :NPP.NPP_NPAY_ID := 1;&amp;#10;    &#x9; END IF;&amp;#10;    END IF;&#x9;&#x9;      &amp;#10;   END IF; &amp;#10;*/&amp;#10;&#x9;&amp;#10;   &amp;#10;-- Paygroup 2 (FC-NON TAXED SUPPLIERS)&amp;#10;ELSIF :NADD.NADD_CIA2_ID &lt;> 'GB' AND :NADD.NADD_ATYP_CODE = 'H' AND &amp;#10;      :NPP.NPP_HMRC_INT_Q3 = 'N' AND&amp;#10;      :NPP.NPP_HMRC_INT_Q4 = 'N' AND&amp;#10;      :NPP.NPP_HMRC_INT_Q5 = 'N' AND&amp;#10;      :NPP.NPP_HMRC_INT_Q6 = 'N' AND&amp;#10;      :NPP.NPP_HMRC_INT_Q7 &lt;> 'GBP' AND&amp;#10;      :NPP.NPP_HMRC_INT_Q8 = 'N'&amp;#10;THEN&amp;#10;-- EC4982 Commented the below logic and added the assigning of Paygroup 2 &amp;#10;   --EC1922&amp;#10;  /* IF :NPP.COPY_NPP_NPAY_ID IS NOT NULL &amp;#10;   THEN&amp;#10;   &#x9;IF :NPP.COPY_NPP_NPAY_ID = 3 &amp;#10;   &#x9;THEN&amp;#10;   &#x9;&#x9;IF:NPP.NPP_HMRC_INT_Q7 &lt;> 'GBP'&amp;#10;   &#x9;&#x9;THEN&amp;#10;         :NPP.NPP_NPAY_ID := 2;&amp;#10;   &#x9;&#x9;END IF;&amp;#10;   &#x9;ELSIF :NPP.COPY_NPP_NPAY_ID = 1 &amp;#10;   &#x9;THEN&amp;#10;   &#x9;  IF :NPP.NPP_HMRC_INT_Q7 &lt;> 'GBP'&amp;#10;   &#x9;&#x9;THEN&amp;#10;         :NPP.NPP_NPAY_ID := 2;&amp;#10;   &#x9;&#x9;END IF;&#x9;&amp;#10;   &#x9;END IF;&amp;#10;   &#x9;&amp;#10;   ELSE&amp;#10;   &#x9;&#x9;IF  :NPP.NPP_HMRC_INT_Q7 &lt;> 'GBP' &amp;#10;   &#x9;&#x9;THEN &amp;#10;         :NPP.NPP_NPAY_ID := 2;&amp;#10;      END IF;&amp;#10;   END IF;&#x9;&#x9;&amp;#10;   */&amp;#10;   &amp;#10;    :NPP.NPP_NPAY_ID := 2;&amp;#10;&amp;#10;-- Paygroup 3 (SUPPLIERS)&amp;#10;ELSIF :NADD.NADD_CIA2_ID &lt;> 'GB' AND :NADD.NADD_ATYP_CODE = 'H' AND&amp;#10;&#x9;         :NPP.NPP_HMRC_INT_Q3 = 'N' AND&amp;#10;           :NPP.NPP_HMRC_INT_Q4 = 'N' AND&amp;#10;           :NPP.NPP_HMRC_INT_Q5 = 'N' AND&amp;#10;           :NPP.NPP_HMRC_INT_Q6 = 'N' AND&amp;#10;           :NPP.NPP_HMRC_INT_Q7 = 'GBP' AND&amp;#10;           :NPP.NPP_HMRC_INT_Q8 = 'N'&amp;#10;THEN&amp;#10;   --EC1922&amp;#10;-- EC4982 Commented the below logic &amp;#10;/*&amp;#10;   IF :NPP.COPY_NPP_NPAY_ID IS NOT NULL &amp;#10;   THEN&#x9;&amp;#10;   &#x9;IF :NPP.COPY_NPP_NPAY_ID = 2 &amp;#10;   &#x9;THEN&amp;#10;   &#x9;  &#x9;IF :NPP.COPY_NPP_HMRC_INT_Q7 = 'GBP'&amp;#10;   &#x9;  &#x9;THEN &amp;#10;           :NPP.NPP_NPAY_ID := 3;&amp;#10;   &#x9;  &#x9;END IF;&amp;#10;    END IF;&amp;#10;   &#x9;&amp;#10;   ELSE&#x9;*/&amp;#10;   &#x9;  :NPP.NPP_NPAY_ID := 3;&amp;#10;   --END IF;&#x9;  &amp;#10;&amp;#10;         &#x9;&amp;#10;-- Paygroup 4 (UK TAXED GBP)&amp;#10;ELSIF :NADD.NADD_CIA2_ID &lt;> 'GB' AND &amp;#10;   &#x9;  :NADD.NADD_ATYP_CODE = 'H' AND&amp;#10;   &#x9;  :NPP.NPP_HMRC_INT_Q6 IN ('Y','N') AND &#x9;  &amp;#10;   &#x9;  :NPP.NPP_HMRC_INT_Q7 = 'GBP'   &#x9;  &amp;#10;   &#x9;  &amp;#10;THEN&amp;#10;-- EC4982 Commented the below logic AND ADDED THE CHECK FOR Q3, Q4, Q5 &amp; Q8 IN IF CONDITION&amp;#10;&#x9;&amp;#10;&#x9;IF  nvl(:NPP.NPP_HMRC_INT_Q3,'N') = 'Y'  OR&amp;#10;      nvl(:NPP.NPP_HMRC_INT_Q4,'N') = 'Y'  OR&amp;#10;      nvl(:NPP.NPP_HMRC_INT_Q5,'N') = 'Y'  OR&amp;#10;      nvl(:NPP.NPP_HMRC_INT_Q8,'N')  = 'Y' &amp;#10;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;:NPP.NPP_NPAY_ID := 4;&amp;#10;&#x9;ELSE&amp;#10;&#x9;&#x9; SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9; 'WARNING !! Either Q1 or Q2 or Q3 or Q6 should be Y for Paygroup 4 ...'||&amp;#10;                 '...Please change accordingly .');&amp;#10;&amp;#10;&#x9;&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&amp;#10;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;  &#x9;THEN &amp;#10;  &#x9;&#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&#x9;END IF ;&amp;#10;/*&amp;#10;   IF :NPP.COPY_NPP_NPAY_ID IS NOT NULL  &amp;#10;   THEN&#x9;&amp;#10;     IF :NPP.COPY_NPP_NPAY_ID = 1 THEN&amp;#10;     &#x9; IF v_old_nadd_cia2_id = 'GB' THEN&amp;#10;     &#x9; &#x9;   :NPP.NPP_NPAY_ID := 4;&amp;#10;     &#x9; END IF;&amp;#10;       &amp;#10;     ELSIF :NPP.COPY_NPP_NPAY_ID = 5 THEN&amp;#10;   &#x9;   IF :NPP.COPY_NPP_HMRC_INT_Q7 &lt;> 'GBP' &amp;#10;   &#x9;   THEN&amp;#10;   &#x9;  &#x9;&#x9; :NPP.NPP_NPAY_ID := 4;&amp;#10;   &#x9;   END IF;&amp;#10;   &#x9; &amp;#10;     ELSIF :NPP.COPY_NPP_NPAY_ID = 2 THEN&amp;#10;   &#x9;    IF :NPP.COPY_NPP_HMRC_INT_Q5 = 'N' AND :NPP.NPP_HMRC_INT_Q5 = 'Y' &amp;#10;   &#x9;    THEN&amp;#10;   &#x9;       :NPP.NPP_NPAY_ID := 4;&amp;#10;   &#x9;    END IF;&amp;#10;   &#x9;  END IF;&amp;#10;   &amp;#10;   ELSE  &amp;#10;      IF (:NPP.NPP_HMRC_INT_Q3 = 'Y' OR&amp;#10;&#x9;        :NPP.NPP_HMRC_INT_Q4 = 'Y' OR&amp;#10;&#x9;        :NPP.NPP_HMRC_INT_Q5 = 'Y' OR&amp;#10;&#x9;        :NPP.NPP_HMRC_INT_Q6 = 'Y' OR&amp;#10;&#x9;        :NPP.NPP_HMRC_INT_Q8 = 'Y') &amp;#10;&#x9;        AND&amp;#10;&#x9;        ( :NPP.NPP_HMRC_INT_Q3 IS NOT NULL AND&amp;#10;&#x9;          :NPP.NPP_HMRC_INT_Q4 IS NOT NULL AND&amp;#10;&#x9;          :NPP.NPP_HMRC_INT_Q5 IS NOT NULL AND&amp;#10;&#x9;          :NPP.NPP_HMRC_INT_Q8 IS NOT NULL)&amp;#10;&#x9;    THEN&amp;#10;&#x9;    &amp;#10;&#x9;        :NPP.NPP_NPAY_ID := 4;&amp;#10;      END IF;&amp;#10;    END IF;&amp;#10;    */&amp;#10;  &amp;#10;               &amp;#10;-- Paygroup 5 (UK TAXED FOREIGN CURRENCY)&amp;#10;ELSIF :NADD.NADD_CIA2_ID &lt;> 'GB' AND &amp;#10;    &#x9;:NADD.NADD_ATYP_CODE = 'H' AND &amp;#10;    &#x9;:NPP.NPP_HMRC_INT_Q6 = 'N' AND&amp;#10;    &#x9;:NPP.NPP_HMRC_INT_Q7 &lt;> 'GBP' &amp;#10;THEN&amp;#10;-- EC4982 Commented the below logic AND ADDED THE CHECK FOR Q3, Q4, Q5 &amp; Q8 IN IF CONDITION&amp;#10;  /*  --EC1922&amp;#10;   IF :NPP.COPY_NPP_NPAY_ID IS NOT NULL &amp;#10;   THEN&amp;#10;   &#x9;IF :NPP.COPY_NPP_NPAY_ID = 4 &amp;#10;   &#x9;THEN&amp;#10;   &#x9;&#x9;IF :NPP.COPY_NPP_HMRC_INT_Q7 = 'GBP'&amp;#10;   &#x9;&#x9;THEN&amp;#10;   &#x9;&#x9;    :NPP.NPP_NPAY_ID := 5;&amp;#10;   &#x9;&#x9;END IF;&amp;#10;   &#x9;ELSIF :NPP.COPY_NPP_NPAY_ID = 3 &amp;#10;   &#x9;THEN&amp;#10;   &#x9;&#x9;IF :NPP.COPY_NPP_HMRC_INT_Q5 = 'N' AND&amp;#10;   &#x9;&#x9;&#x9; :NPP.NPP_HMRC_INT_Q5 = 'Y'&amp;#10;   &#x9;&#x9;THEN&amp;#10;   &#x9;&#x9;    :NPP.NPP_NPAY_ID := 5;&amp;#10;   &#x9;&#x9;END IF;&amp;#10;   &#x9;--EC2121&amp;#10;   &#x9;ELSIF :NPP.COPY_NPP_NPAY_ID = 1 &amp;#10;   &#x9;THEN&amp;#10;   &#x9;    :NPP.NPP_NPAY_ID := 5;  &amp;#10;   &#x9;END IF; &amp;#10;   ELSE*/&amp;#10;     IF nvl(:NPP.NPP_HMRC_INT_Q3, 'N') = 'Y' OR&amp;#10;&#x9;       nvl(:NPP.NPP_HMRC_INT_Q4, 'N') = 'Y' OR&amp;#10;&#x9;       nvl(:NPP.NPP_HMRC_INT_Q5, 'N') = 'Y' OR&amp;#10;&#x9;       nvl(:NPP.NPP_HMRC_INT_Q8, 'N') = 'Y' &#x9;&amp;#10;&#x9;   THEN&amp;#10;&#x9;      :NPP.NPP_NPAY_ID := 5;&amp;#10;&#x9;   ELSE&amp;#10;&#x9;&#x9; &#x9;&#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9; 'WARNING !! Either Q1 or Q2 or Q3 or Q6 should be Y for Paygroup 5 ...'||&amp;#10;                 '...Please change accordingly .');&amp;#10;&amp;#10;&#x9;&#x9;&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&amp;#10;&#x9;&#x9;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;&#x9;  &#x9;&#x9;THEN &amp;#10;&#x9;  &#x9;&#x9;&#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;&#x9;&amp;#10;     END IF;&amp;#10;   --END IF;&amp;#10;ELSE&amp;#10;&#x9;-- EC4982 added the below else statement&amp;#10;&#x9;&amp;#10;-- EC5395 - Added new IF condition to show alert only if its NON GB&amp;#10;&#x9;&amp;#10;&#x9;IF :NADD.NADD_CIA2_ID &lt;> 'GB' AND :NPP.NPP_NPAY_ID NOT IN (2, 3, 4, 5 )&amp;#10;&#x9;THEN&amp;#10;&#x9;&amp;#10;&#x9;&#x9;&#x9;&#x9;SET_ALERT_PROPERTY('CFG_WARNING',ALERT_MESSAGE_TEXT,&amp;#10;&#x9;&#x9;&#x9;&#x9;'WARNING !! Appropriate HMRC Qs response should be entered for Paygroup re-calculation ...'||&amp;#10;&#x9;&#x9;&#x9;&#x9;   '...Please change accordingly - PROC .'|| :NADD.NADD_CIA2_ID);&amp;#10;&#x9;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;&#x9;&#x9;SET_APPLICATION_PROPERTY(CURSOR_STYLE,'DEFAULT');&amp;#10;&#x9;&#x9;&#x9;&#x9;&amp;#10;&#x9;&#x9;&#x9;&#x9;IF SHOW_ALERT('CFG_WARNING') = ALERT_BUTTON1&amp;#10;&#x9;&#x9;&#x9;&#x9;THEN &amp;#10;&#x9;&#x9;&#x9;&#x9;&#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;&#x9;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;END IF;&amp;#10;&#x9;&amp;#10;-- EC5395 - Added new IF condition to show alert only if its NON GB&amp;#10;&#x9;&amp;#10;END IF; -- Paygroup set   &#x9;&#x9;&#x9;&amp;#10;    &#x9;&#x9;&#x9;&amp;#10;END;&amp;#10;&amp;#10;declare&amp;#10;&#x9;cursor c_npay is&amp;#10;  SELECT NPAY_DESCRIPTION&amp;#10;  FROM NPP_PAYGROUPS&amp;#10;  WHERE NPAY_ID = :NPP.NPP_NPAY_ID;&amp;#10;  &amp;#10;begin&amp;#10;&#x9;&amp;#10;&#x9;open c_npay;&amp;#10;&#x9;fetch c_npay into :NPP.NBT_NPAY_DESCRIPTION;&amp;#10;&#x9;close c_npay;&amp;#10;&#x9;&amp;#10;exception&amp;#10;&#x9; when no_data_found then null;&amp;#10;end;&amp;#10;END;" ProgramUnitType="Procedure"/>
