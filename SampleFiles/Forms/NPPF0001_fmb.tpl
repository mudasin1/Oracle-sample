         <Trigger Name="ON-CHECK-DELETE-MASTER" TriggerText="--&amp;#10;-- Begin default relation declare section&amp;#10;--&amp;#10;DECLARE&amp;#10;  Dummy_Define VARCHAR2(1);&amp;#10;  --&amp;#10;  -- Begin NRCE detail declare section&amp;#10;  --&amp;#10;  CURSOR NRCE_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_CENTRE_EXCLUSIONS     &amp;#10;    WHERE NRCE_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRCE detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRBU detail declare section&amp;#10;  --&amp;#10;  CURSOR NRBU_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_BUSINESS_UNITS     &amp;#10;    WHERE NRBU_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRBU detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRTE detail declare section&amp;#10;  --&amp;#10;  CURSOR NRTE_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_TEAMS     &amp;#10;    WHERE NRTE_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRTE detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRGS detail declare section&amp;#10;  --&amp;#10;  CURSOR NRGS_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_GNVQ_SUBJECT_LEVELS     &amp;#10;    WHERE NRGS_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRGS detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRGU detail declare section&amp;#10;  --&amp;#10;  CURSOR NRGU_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_GNVQ_UNIT_EXCLUSIONS     &amp;#10;    WHERE NRGU_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRGU detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRRE1 detail declare section&amp;#10;  --&amp;#10;  CURSOR NRRE1_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_REVIEWS     &amp;#10;    WHERE NRRE_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRRE1 detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRRE2 detail declare section&amp;#10;  --&amp;#10;  CURSOR NRRE2_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_REVIEWS     &amp;#10;    WHERE NRRE_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRRE2 detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRWA detail declare section&amp;#10;  --&amp;#10;  CURSOR NRWA_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_WORK_ALLOCATION     &amp;#10;    WHERE NRWA_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRWA detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NREG detail declare section&amp;#10;  --&amp;#10;  CURSOR NREG_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_REGIONS     &amp;#10;    WHERE NREG_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NREG detail declare section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRAV detail declare section&amp;#10;  --&amp;#10;  CURSOR NRAV_cur IS      &amp;#10;    SELECT 1 FROM NPP_ROLE_AVAILABILITY     &amp;#10;    WHERE NRAV_NROL_ID = :NROL.NROL_ID;&amp;#10;  --&amp;#10;  -- End NRAV detail declare section&amp;#10;  --&amp;#10;--&amp;#10;-- End default relation declare section&amp;#10;--&amp;#10;--&amp;#10;-- Begin default relation program section&amp;#10;--&amp;#10;BEGIN&amp;#10;  --&amp;#10;  -- Begin NRCE detail program section&amp;#10;  --&amp;#10;  OPEN NRCE_cur;     &amp;#10;  FETCH NRCE_cur INTO Dummy_Define;     &amp;#10;  IF ( NRCE_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRCE_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRCE_cur;&amp;#10;  --&amp;#10;  -- End NRCE detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRBU detail program section&amp;#10;  --&amp;#10;  OPEN NRBU_cur;     &amp;#10;  FETCH NRBU_cur INTO Dummy_Define;     &amp;#10;  IF ( NRBU_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRBU_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRBU_cur;&amp;#10;  --&amp;#10;  -- End NRBU detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRTE detail program section&amp;#10;  --&amp;#10;  OPEN NRTE_cur;     &amp;#10;  FETCH NRTE_cur INTO Dummy_Define;     &amp;#10;  IF ( NRTE_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRTE_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRTE_cur;&amp;#10;  --&amp;#10;  -- End NRTE detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRGS detail program section&amp;#10;  --&amp;#10;  OPEN NRGS_cur;     &amp;#10;  FETCH NRGS_cur INTO Dummy_Define;     &amp;#10;  IF ( NRGS_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRGS_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRGS_cur;&amp;#10;  --&amp;#10;  -- End NRGS detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRGU detail program section&amp;#10;  --&amp;#10;  OPEN NRGU_cur;     &amp;#10;  FETCH NRGU_cur INTO Dummy_Define;     &amp;#10;  IF ( NRGU_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRGU_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRGU_cur;&amp;#10;  --&amp;#10;  -- End NRGU detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRRE1 detail program section&amp;#10;  --&amp;#10;  OPEN NRRE1_cur;     &amp;#10;  FETCH NRRE1_cur INTO Dummy_Define;     &amp;#10;  IF ( NRRE1_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRRE1_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRRE1_cur;&amp;#10;  --&amp;#10;  -- End NRRE1 detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRRE2 detail program section&amp;#10;  --&amp;#10;  OPEN NRRE2_cur;     &amp;#10;  FETCH NRRE2_cur INTO Dummy_Define;     &amp;#10;  IF ( NRRE2_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRRE2_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRRE2_cur;&amp;#10;  --&amp;#10;  -- End NRRE2 detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRWA detail program section&amp;#10;  --&amp;#10;  OPEN NRWA_cur;     &amp;#10;  FETCH NRWA_cur INTO Dummy_Define;     &amp;#10;  IF ( NRWA_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRWA_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRWA_cur;&amp;#10;  --&amp;#10;  -- End NRWA detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NREG detail program section&amp;#10;  --&amp;#10;  OPEN NREG_cur;     &amp;#10;  FETCH NREG_cur INTO Dummy_Define;     &amp;#10;  IF ( NREG_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NREG_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NREG_cur;&amp;#10;  --&amp;#10;  -- End NREG detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRAV detail program section&amp;#10;  --&amp;#10;  OPEN NRAV_cur;     &amp;#10;  FETCH NRAV_cur INTO Dummy_Define;     &amp;#10;  IF ( NRAV_cur%found ) THEN     &amp;#10;    Message('Cannot delete master record when matching detail records exist.');     &amp;#10;    CLOSE NRAV_cur;     &amp;#10;    RAISE Form_Trigger_Failure;     &amp;#10;  END IF;&amp;#10;  CLOSE NRAV_cur;&amp;#10;  --&amp;#10;  -- End NRAV detail program section&amp;#10;  --&amp;#10;END;&amp;#10;--&amp;#10;-- End default relation program section&amp;#10;--&amp;#10;"/>
         <Trigger Name="ON-POPULATE-DETAILS" TriggerText="--&amp;#10;-- Begin default relation declare section&amp;#10;--&amp;#10;DECLARE&amp;#10;  recstat     VARCHAR2(20) := :System.record_status;   &amp;#10;  startitm    VARCHAR2(61) := :System.cursor_item;   &amp;#10;  rel_id      Relation;&amp;#10;--&amp;#10;-- End default relation declare section&amp;#10;--&amp;#10;--&amp;#10;-- Begin default relation program section&amp;#10;--&amp;#10;BEGIN&amp;#10;  IF ( recstat = 'NEW' or recstat = 'INSERT' ) THEN   &amp;#10;    RETURN;&amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- Begin NRCE detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRCE');   &amp;#10;    Query_Master_Details(rel_id, 'NRCE');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRCE detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRBU detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRBU');   &amp;#10;    Query_Master_Details(rel_id, 'NRBU');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRBU detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRTE detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRTE');   &amp;#10;    Query_Master_Details(rel_id, 'NRTE');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRTE detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRGS detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRGS');   &amp;#10;    Query_Master_Details(rel_id, 'NRGS');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRGS detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRGU detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRGU');   &amp;#10;    Query_Master_Details(rel_id, 'NRGU');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRGU detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRRE1 detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRRE1');   &amp;#10;    Query_Master_Details(rel_id, 'NRRE1');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRRE1 detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRRE2 detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRRE2');   &amp;#10;    Query_Master_Details(rel_id, 'NRRE2');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRRE2 detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRWA detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRWA');   &amp;#10;    Query_Master_Details(rel_id, 'NRWA');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRWA detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NREG detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NREG');   &amp;#10;    Query_Master_Details(rel_id, 'NREG');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NREG detail program section&amp;#10;  --&amp;#10;  --&amp;#10;  -- Begin NRAV detail program section&amp;#10;  --&amp;#10;  IF ( (:NROL.NROL_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('NROL.NROL_NRAV');   &amp;#10;    Query_Master_Details(rel_id, 'NRAV');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End NRAV detail program section&amp;#10;  --&amp;#10;&amp;#10;  IF ( :System.cursor_item &lt;> startitm ) THEN     &amp;#10;     Go_Item(startitm);     &amp;#10;     Check_Package_Failure;     &amp;#10;  END IF;&amp;#10;&amp;#10;END;&amp;#10;--&amp;#10;-- End default relation program section&amp;#10;--&amp;#10;"/>
         <Trigger Name="POST-QUERY" TriggerText=":CONTROL.QUERY_FLAG  := 'Y';&amp;#10;:NROL.NBT_NPP_MODREG_M_CODE := :NPP.NPP_MODREG_M_CODE;&amp;#10;:NROL.NBT_NPP_MODREG_M_MODERATOR_ID := :NPP.NPP_MODREG_M_MODERATOR_ID;&amp;#10;:CONTROL.QUERY_FLAG2 := 'Y';&amp;#10;:NROL.NBT_NPP_CONTACTS_PERS_ID := :NPP.NPP_CONTACTS_PERS_ID;&amp;#10;:NROL.NBT_NPP_CONTACTS_PERS_CODE := :NPP.NPP_CONTACTS_PERS_SHORT_CODE;&amp;#10;BEGIN&amp;#10; SELECT &#x9;NRTY_DESCRIPTION&amp;#10; INTO   &#x9;:NROL.NBT_NRTY_DESCRIPTION&amp;#10; FROM   &#x9;NPP_ROLE_TYPES&amp;#10; WHERE&#x9;&#x9;NRTY_ID = :NROL.NROL_NRTY_ID;&amp;#10; EXCEPTION&amp;#10; WHEN NO_DATA_FOUND THEN NULL;&amp;#10; END;&amp;#10;&amp;#10;PR_GET_ROLE_TYPE_DETAIL;&amp;#10;PR_FORMAT_NO_UNITS;&amp;#10;:NROL.NBT_GRADE := :NROL.NROL_RECRUITMENT_GRADE;&amp;#10;&amp;#10;-- Moved processing D32 - V2&amp;#10;pr_calculate_qcod_codes;&amp;#10;&amp;#10;"/>
         <Trigger Name="PRE-UPDATE" TriggerText="IF&#x9;:NROL.NROL_NRTY_ID  IN (1,106,110) AND&amp;#10;&#x9;:NROL.NROL_END_DATE IS NOT NULL&amp;#10;THEN&amp;#10;&#x9;PR_VALIDATE_QASALLOC;&amp;#10;END IF;&amp;#10;IF&#x9;:NROL.NROL_NRTY_ID = 121&amp;#10;THEN&#x9;PR_VALIDATE_121;&amp;#10;END IF;&amp;#10;PR_VALIDATE_NROL_DATES;&amp;#10;PR_VALIDATE_NROL_STATUS;&amp;#10;PR_VALIDATE_NROL_MODREG;&amp;#10;&amp;#10;:NROL.NROL_LAST_UPDATE_DATE := SYSDATE;&amp;#10;:NROL.NROL_LAST_UPDATE_USER := USER;"/>
         <Trigger Name="POST-UPDATE" TriggerText="IF&#x9;:NROL.NROL_NRTY_ID IN (147,148) AND&amp;#10;        :NROL.NROL_EMPLOYMENT_STATUS &lt;> 'CU'&amp;#10;THEN&amp;#10;&#x9;BEGIN&amp;#10;&amp;#10;&#x9;&#x9;UPDATE&#x9;NPP_ROLES&amp;#10;&#x9;&#x9;SET&#x9;NROL_EMPLOYMENT_STATUS&#x9;= :NROL.NROL_EMPLOYMENT_STATUS,&amp;#10;&#x9;&#x9;&#x9;NROL_END_DATE&#x9;&#x9;= :NROL.NROL_END_DATE&amp;#10;&#x9;&#x9;WHERE&#x9;NROL_NPP_ID &#x9;&#x9;= :NROL.NROL_NPP_ID&amp;#10;&#x9;&#x9;AND&#x9;NROL_EMPLOYMENT_STATUS &lt;> :NROL.NROL_EMPLOYMENT_STATUS&amp;#10;&#x9;&#x9;AND&#x9;NROL_NRTY_ID+0&#x9;&#x9;= DECODE(:NROL.NROL_NRTY_ID,147,148,147);&amp;#10;&amp;#10;&#x9;&#x9;IF&#x9;SQL%ROWCOUNT > 0&amp;#10;&#x9;&#x9;THEN&amp;#10;&#x9;&#x9;&#x9;IF&#x9;:NROL.NROL_NRTY_ID = 147&amp;#10;&#x9;&#x9;&#x9;THEN&#x9;MSG_ALERT('BTEC VERIFIER ROLE HAS ALSO BEEN UPDATED TO NOT CURRENT ','I',FALSE);&amp;#10;&#x9;&#x9;&#x9;ELSE&#x9;MSG_ALERT('VAM ROLE HAS ALSO BEEN UPDATED TO NOT CURRENT ','I',FALSE);&amp;#10;&#x9;&#x9;&#x9;END IF;&amp;#10;&#x9;&#x9;END IF;&amp;#10;&amp;#10;&#x9;END;&amp;#10;END IF;&amp;#10;&amp;#10;"/>
         <Trigger Name="PRE-INSERT" TriggerText="DECLARE DUMMY_FIELD VARCHAR2(1);&amp;#10;&amp;#10;BEGIN&amp;#10; SELECT NULL&amp;#10; INTO   DUMMY_FIELD&amp;#10; FROM   NPP_ROLES&amp;#10; WHERE  NROL_NPP_ID  &#x9;= :NROL.NROL_NPP_ID&amp;#10; AND    NROL_NRTY_ID &#x9;= :NROL.NROL_NRTY_ID;&amp;#10;&amp;#10; SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;        'DUPLICATE ROLE FOR NPP');&amp;#10; IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10; THEN RAISE FORM_TRIGGER_FAILURE;&amp;#10; END IF;&amp;#10;&amp;#10; EXCEPTION&amp;#10;  WHEN NO_DATA_FOUND THEN NULL;&amp;#10;&amp;#10;END;&amp;#10;&amp;#10;IF&#x9;:NROL.NROL_NRTY_ID = 121&amp;#10;THEN&#x9;PR_VALIDATE_121;&amp;#10;END IF;&amp;#10;PR_VALIDATE_NROL_DATES;&amp;#10;PR_VALIDATE_NROL_STATUS;&amp;#10;PR_VALIDATE_NROL_MODREG;&amp;#10;&amp;#10;--IF :NROL.NROL_NRTY_ID IN (1,106,110,114,147,152,153,165,171,216,217,300)&amp;#10;IF   :NROL.NBT_NRTY_VOCATIONAL_IND = 'Y'&amp;#10;THEN&amp;#10;&amp;#10; BEGIN&amp;#10; &amp;#10;  SELECT&#x9;NRTY_D_MAX_CENTRES&amp;#10;  INTO&#x9;&#x9;:NROL.NROL_MAX_CENTRES&amp;#10;  FROM&#x9;&#x9;NPP_ROLE_TYPES&amp;#10;  WHERE&#x9;&#x9;NRTY_ID &#x9;&#x9;&#x9;= :NROL.NROL_NRTY_ID;&amp;#10;  &amp;#10;  MSG_ALERT('INFORMATION...Maximum Centres Has Been Automatically Set To '||&amp;#10;  &#x9;&#x9;TO_CHAR(:NROL.NROL_MAX_CENTRES),'I',FALSE);&amp;#10;&amp;#10; EXCEPTION&amp;#10; &#x9;&amp;#10; &#x9;WHEN NO_DATA_FOUND THEN NULL;&amp;#10;&amp;#10; END;&amp;#10;&amp;#10;END IF;&amp;#10;&amp;#10;BEGIN&amp;#10;&amp;#10; SELECT NROL_SEQ.NEXTVAL,&amp;#10;        SYSDATE,&amp;#10;        USER&amp;#10; INTO   :NROL.NROL_ID,&amp;#10;        :NROL.NROL_INSERT_DATE,&amp;#10;        :NROL.NROL_INSERT_USER&amp;#10; FROM   DUAL;&amp;#10;&amp;#10;END;&amp;#10;&amp;#10;"/>
         <Trigger Name="POST-INSERT" TriggerText="IF :NROL.NBT_NRTY_AS400_IND = 'Y'&amp;#10;THEN&amp;#10; BEGIN&amp;#10;  INSERT INTO NPP_ROLE_CENTRE_EXCLUSIONS&amp;#10;  (NRCE_NROL_ID,&amp;#10;   NRCE_CENTRE_ID,&amp;#10;   NRCE_REASON_CODE,&amp;#10;   NRCE_INSERT_DATE,&amp;#10;   NRCE_INSERT_USER)&amp;#10;  SELECT DISTINCT&amp;#10;         :NROL.NROL_ID,&amp;#10;         NRCE_CENTRE_ID,&amp;#10;         NRCE_REASON_CODE,&amp;#10;         TRUNC(SYSDATE),&amp;#10;         USER&amp;#10;  FROM   NPP_ROLE_CENTRE_EXCLUSIONS,&amp;#10;         NPP_ROLE_TYPES,&amp;#10;         NPP_ROLES&amp;#10;  WHERE  NRCE_NROL_ID   = NROL_ID+0&amp;#10;  AND    NRTY_AS400_IND = 'Y'&amp;#10;  AND    NRTY_ID&#x9;= NROL_NRTY_ID+0&amp;#10;  AND    NROL_NPP_ID    = :NPP.NPP_ID&amp;#10;  AND    NROL_NRTY_ID  &lt;> :NROL.NROL_NRTY_ID;&amp;#10;  EXCEPTION&amp;#10;   WHEN NO_DATA_FOUND THEN NULL;&amp;#10; END;&amp;#10;END IF;&amp;#10;&amp;#10;--IF :NROL.NROL_NRTY_ID IN (1,106,110,114,147,153,165,200,526,216,217,300,551)&amp;#10;IF   :NROL.NBT_NRTY_VOCATIONAL_IND = 'Y'&amp;#10;THEN&amp;#10; BEGIN&amp;#10;  INSERT INTO NPP_ROLE_PERFORMANCE&amp;#10;  (NRPE_NROL_ID,&amp;#10;   NRPE_DATE,&amp;#10;   NRPE_GRADE,&amp;#10;   NRPE_INSERT_DATE,&amp;#10;   NRPE_INSERT_USER)&amp;#10;  VALUES&amp;#10;  (:NROL.NROL_ID,&amp;#10;   :NROL.NROL_START_DATE,&amp;#10;   'D',&amp;#10;   TRUNC(SYSDATE),&amp;#10;   USER);&amp;#10;  END;&amp;#10;END IF;&amp;#10;&amp;#10;"/>
         <Trigger Name="KEY-DELREC" TriggerText="IF :NROL.ROWID IS NULL&amp;#10;THEN DELETE_RECORD;&amp;#10;ELSE&amp;#10; SET_ALERT_PROPERTY('CFG_ERROR',ALERT_MESSAGE_TEXT,&amp;#10;                   'ROLES CANNOT BE DELETED');&amp;#10; IF SHOW_ALERT('CFG_ERROR') = ALERT_BUTTON1&amp;#10; THEN NULL;&amp;#10; END IF;&amp;#10;END IF;"/>
         <Trigger Name="KEY-ENTQRY" TriggerText="EXECUTE_QUERY;"/>
