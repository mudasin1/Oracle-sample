         <Trigger Name="POST-QUERY" DirtyInfo="true" TriggerText="&amp;#10;declare&amp;#10;&#x9;&amp;#10;&#x9;cursor c_reg &amp;#10;&#x9;is&amp;#10;&#x9;  select st_forenames||' '||st_surname&amp;#10;&#x9;  ,      st_centre_id&amp;#10;&#x9;  ,      st_scheme_reg_no&amp;#10;&#x9;  from students&amp;#10;&#x9;  where st_reg_no = :sare.sare_st_reg_no;&amp;#10;&#x9;  &amp;#10;&#x9;cursor c_centre (cp_centre_id centres.cn_centre_id%type)&amp;#10;&#x9;is&amp;#10;&#x9;  select cn_centre_name&amp;#10;&#x9;  from centres&amp;#10;&#x9;  where cn_centre_id = cp_centre_id;&amp;#10;&#x9;  &amp;#10;&#x9; cursor c_asse&amp;#10;&#x9; is&amp;#10;&#x9;    select asse_catalogue_name&amp;#10;&#x9;    ,      asse_desc&amp;#10;&#x9;    ,      atru_tariffs_possible&amp;#10;&#x9;    from   award_title_rules,&amp;#10;             assessments&amp;#10;&#x9;    where  asse_id = :sare.sare_asse_id&amp;#10;&#x9;    and    atru_id = asse_atru_id;&amp;#10;&#x9;    &amp;#10;&#x9; cursor c_aout&amp;#10;&#x9; is&amp;#10;&#x9;    select aout_desc&amp;#10;&#x9;    from assessment_outcomes&amp;#10;&#x9;    where aout_id = :sare.sare_aout_id;&amp;#10;&#x9;    &amp;#10;&#x9; cursor c_aout_ng&amp;#10;&#x9; is&amp;#10;&#x9;    select aout_desc&amp;#10;&#x9;    from assessment_outcomes&amp;#10;&#x9;    where aout_id = :sare.sare_aout_id&amp;#10;&#x9;    and :sare.sare_aout_id not in ('U','W')&amp;#10;&#x9;    union&amp;#10;&#x9;    select 'UNCLASSIFIED'&amp;#10;&#x9;    from dual&amp;#10;&#x9;    where :sare.sare_aout_id = 'U'&amp;#10;&#x9;    and exists&amp;#10;&#x9;    (select null from units,assessments,assessment_units &amp;#10;&#x9;     where asse_id = :sare_asse_id&amp;#10;&#x9;     and   un_unit_code = auni_un_unit_code &amp;#10;&#x9;     and   auni_asse_id = asse_id&amp;#10;&#x9;     and   un_assessment = 'G')&amp;#10;&#x9;    union&amp;#10;&#x9;    select 'FAIL'&amp;#10;&#x9;    from dual&amp;#10;&#x9;    where :sare.sare_aout_id = 'U'&amp;#10;&#x9;    and not exists&amp;#10;&#x9;    (select null from units,assessments,assessment_units &amp;#10;&#x9;     where asse_id = :sare_asse_id &amp;#10;&#x9;     and un_unit_code = auni_un_unit_code &amp;#10;&#x9;     and   auni_asse_id = asse_id&amp;#10;&#x9;     and un_assessment = 'G')&amp;#10;&#x9;    union&amp;#10;&#x9;    select 'PROVISIONALLY '||aout_desc&amp;#10;&#x9;    from assessment_outcomes&amp;#10;&#x9;    where aout_id = :sare.sare_pending_grade&amp;#10;&#x9;    and :sare.sare_aout_id = 'W'&amp;#10;&#x9;    and :sare.sare_pending_grade &lt;> 'U'&amp;#10;&#x9;    union&amp;#10;&#x9;    select 'PROVISIONALLY UNCLASSIFIED'&amp;#10;&#x9;    from dual&amp;#10;&#x9;    where :sare.sare_aout_id = 'W'&amp;#10;&#x9;    and :sare.sare_pending_grade = 'U'&amp;#10;&#x9;    and exists&amp;#10;&#x9;    (select null &amp;#10;&#x9;     from units,assessments,assessment_units&amp;#10;&#x9;     where asse_id = :sare_asse_id &amp;#10;&#x9;     and un_unit_code = auni_un_unit_code &amp;#10;&#x9;     and auni_asse_id=asse_id and un_assessment = 'G')&amp;#10;&#x9;    union&amp;#10;&#x9;    select 'PROVISIONALLY FAIL'&amp;#10;&#x9;    from dual&amp;#10;&#x9;    where :sare.sare_aout_id = 'W'&amp;#10;&#x9;    and :sare.sare_pending_grade = 'U'&amp;#10;&#x9;    and not exists&amp;#10;&#x9;    (select null from units,assessments,assessment_units&amp;#10;&#x9;     where asse_id = :sare_asse_id &amp;#10;&#x9;     and un_unit_code =  auni_un_unit_code &amp;#10;&#x9;     and auni_asse_id=asse_id &amp;#10;&#x9;     and un_assessment = 'G');&amp;#10;&#x9;    &amp;#10;&#x9; cursor c_atar&amp;#10;&#x9; is&amp;#10;&#x9;    select atar_desc&amp;#10;&#x9;    from assessment_tariffs&amp;#10;&#x9;    where atar_id = :sare.sare_atar_id;&#x9;    &amp;#10;&#x9;    &amp;#10;&#x9; cursor c_arfi&amp;#10;&#x9; is&amp;#10;&#x9;    select arfi_file_datetime,&amp;#10;&#x9;           arfi_load_date&amp;#10;&#x9;    from assessment_results_files&amp;#10;&#x9;    where arfi_id = :sare.sare_arfi_id&amp;#10;&#x9;    and   arfi_id &lt;> 0;&#x9;    &amp;#10;&#x9;    &amp;#10;begin&amp;#10;&#x9;&amp;#10;&#x9;  if :sare.sare_st_reg_no is not null&amp;#10;&#x9;  then&amp;#10;&#x9;  &amp;#10;&#x9;    open c_reg;&amp;#10;&#x9;    fetch c_reg into :sare.nbt_st_name&amp;#10;&#x9;    ,                :sare.nbt_cn_centre_id&amp;#10;&#x9;    ,                :sare.nbt_scheme_reg_no;&amp;#10;&#x9;    close c_reg;&amp;#10;&#x9;  end if;&amp;#10;&#x9;  &amp;#10;&#x9;  if :sare.nbt_cn_centre_id is not null &amp;#10;&#x9;  then&amp;#10;&#x9;     open c_centre (:sare.nbt_cn_centre_id);&amp;#10;&#x9;     fetch c_centre into :sare.nbt_cn_centre_name;&amp;#10;&#x9;     close c_centre;&amp;#10;&#x9;  end if;&amp;#10;&#x9;  &amp;#10;&#x9;  if :sare.sare_asse_id is not null&amp;#10;&#x9;  then&amp;#10;&#x9;  &amp;#10;&#x9;    open c_asse;&amp;#10;&#x9;    fetch c_asse into :sare.nbt_asse_catalogue_name&amp;#10;&#x9;    ,                 :sare.nbt_asse_desc&amp;#10;&#x9;    ,                 :sare.nbt_atru_tariffs_possible;&amp;#10;&#x9;    close c_asse;&amp;#10;&#x9;  end if;&amp;#10;&#x9;  &amp;#10;&#x9;  if :sare.sare_aout_id is not null&amp;#10;&#x9;  then&amp;#10;&#x9;  &amp;#10;&#x9;    if (:sare.sare_pending_grade is not null or :sare.sare_urn_id like '%PQSS0031%')&amp;#10;&#x9;    then&amp;#10;&#x9;    &#x9;open c_aout_ng;&amp;#10;&#x9;    &#x9;fetch c_aout_ng into :sare.nbt_aout_desc;&amp;#10;&#x9;    &#x9;close c_aout_ng;&amp;#10;&#x9;    else&amp;#10;&#x9;    &#x9;open c_aout;&amp;#10;&#x9;    &#x9;fetch c_aout into :sare.nbt_aout_desc;&amp;#10;&#x9;    &#x9;close c_aout;&amp;#10;&#x9;    end if;&amp;#10;&#x9;  end if;&amp;#10;&#x9;  &#x9;  &amp;#10;&#x9;  if :sare.sare_atar_id is not null&amp;#10;&#x9;  then&amp;#10;&#x9;    open c_atar;&amp;#10;&#x9;    fetch c_atar into :sare.nbt_atar_desc;&amp;#10;&#x9;    close c_atar;&amp;#10;&#x9;  end if;&amp;#10;&#x9;  &#x9;  &amp;#10;&#x9;  if :sare.sare_arfi_id is not null&amp;#10;&#x9;  then&amp;#10;&#x9;    open c_arfi;&amp;#10;&#x9;    fetch c_arfi into :sare.nbt_arfi_datetime,&amp;#10;&#x9;                      :sare.NBT_ARFI_LOAD_DATE;&amp;#10;&#x9;    close c_arfi;&amp;#10;&#x9;  end if;&amp;#10;&#x9;  &#x9;  &amp;#10;&#x9;  exception&amp;#10;&#x9;  &#x9;when no_data_found then&amp;#10;&#x9;  &#x9;   null;&amp;#10;&#x9;  end;&amp;#10;&#x9;  &amp;#10;&#x9;  --EC2531&amp;#10;DECLARE   &#x9;&amp;#10; cursor c_aw_code is&amp;#10; select aw_award_code,nvl(ac_n_grade_activated,'N')&amp;#10; from  students,approval_awards,award_codes&amp;#10; where st_course_id = aw_course_number&amp;#10; and   aw_award_code = ac_code&amp;#10; and st_reg_no =  :SARE.SARE_ST_REG_NO;&amp;#10;&amp;#10;&amp;#10; &amp;#10; n_assessments_complete number(15) := 0;&amp;#10;BEGIN&amp;#10;&#x9;&amp;#10;&#x9;open c_aw_code;&amp;#10;&#x9;fetch c_aw_code into :sare.nbt_ac_code,:sare.nbt_ac_n_grade_activated;&amp;#10;&#x9;close c_aw_code;&amp;#10;&#x9;&amp;#10;&#x9;IF :SARE.SARE_ID IS NOT NULL&amp;#10;&#x9;THEN&amp;#10;&#x9; --  IF :SARE.NBT_AC_CODE IN ('1N','2N','3N','4N','5N','6N')&amp;#10;&#x9; --  THEN&amp;#10;&#x9;  --EC2772&amp;#10;&#x9;  IF NVL(:SARE.NBT_AC_N_GRADE_ACTIVATED,'N') = 'Y'&amp;#10;&#x9;  THEN&amp;#10;   &amp;#10;   &#x9; &#x9; &amp;#10;   &#x9; &#x9; n_assessments_complete := btec.fn_assessment_points(:SARE.SARE_ST_REG_NO,:SARE.SARE_ID);&amp;#10;   &#x9; &#x9; &amp;#10;   &#x9; &#x9; if n_assessments_complete &lt; 0 then&amp;#10;   &#x9; &#x9; &#x9;   :SARE.NBT_UMS := null;&amp;#10;   &#x9; &#x9; else&amp;#10;   &#x9; &#x9; &#x9;    :SARE.NBT_UMS :=  n_assessments_complete;&amp;#10;   &#x9; &#x9; end if;&amp;#10;   &#x9; &#x9;     &amp;#10;   &#x9; &#x9; &amp;#10;   &#x9; END IF;&amp;#10;&#x9;END IF;&amp;#10;&#x9;&amp;#10;EXCEPTION&amp;#10;   &#x9; &#x9; WHEN OTHERS THEN NULL;&amp;#10;END;&amp;#10;&amp;#10;&amp;#10;"/>
         <Trigger Name="ON-POPULATE-DETAILS" DirtyInfo="true" TriggerText="--&amp;#10;-- Begin default relation declare section&amp;#10;--&amp;#10;DECLARE&amp;#10;  recstat     VARCHAR2(20) := :System.record_status;   &amp;#10;  startitm    VARCHAR2(61) := :System.cursor_item;   &amp;#10;  rel_id      Relation;&amp;#10;--&amp;#10;-- End default relation declare section&amp;#10;--&amp;#10;--&amp;#10;-- Begin default relation program section&amp;#10;--&amp;#10;BEGIN&amp;#10;  IF ( recstat = 'NEW' or recstat = 'INSERT' ) THEN   &amp;#10;    RETURN;&amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- Begin SIRE detail program section&amp;#10;  --&amp;#10;  IF ( (:SARE.SARE_ID is not null) ) THEN   &amp;#10;    rel_id := Find_Relation('SARE.SARE_SIRE');   &amp;#10;    Query_Master_Details(rel_id, 'SIRE');   &amp;#10;  END IF;&amp;#10;  --&amp;#10;  -- End SIRE detail program section&amp;#10;  --&amp;#10;&amp;#10;  IF ( :System.cursor_item &lt;> startitm ) THEN     &amp;#10;     Go_Item(startitm);     &amp;#10;     Check_Package_Failure;     &amp;#10;  END IF;&amp;#10;END;&amp;#10;--&amp;#10;-- End default relation program section&amp;#10;--&amp;#10;"/>
