         <Trigger Name="WHEN-NEW-RECORD-INSTANCE" DirtyInfo="true" TriggerText="-- Allow tariff to be entered if a tariffable award title, a pass or fail result,&amp;#10;-- a tariff has not already been applied and user has permission...&amp;#10;&amp;#10;if :sare.nbt_atru_tariffs_possible = 'Y' and&amp;#10;&#x9; :sare.sare_aout_id in ('P','U')       and&amp;#10;&#x9; :sare.sare_atar_id is NULL            and&amp;#10;&#x9; fn_tariff_update_allowed = 'Y'&amp;#10;then&amp;#10;  SET_ITEM_PROPERTY('SARE.NBT_ATAR_DESC',ENABLED,PROPERTY_TRUE);&amp;#10;  SET_ITEM_PROPERTY('SARE.NBT_ATAR_DESC',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  SET_ITEM_PROPERTY('SARE.NBT_ATAR_DESC',UPDATEABLE,PROPERTY_TRUE);  &amp;#10;  SET_ITEM_PROPERTY('SARE.SARE_TARIFF_NOTES',ENABLED,PROPERTY_TRUE);&amp;#10;  SET_ITEM_PROPERTY('SARE.SARE_TARIFF_NOTES',NAVIGABLE,PROPERTY_TRUE);&amp;#10;  SET_ITEM_PROPERTY('SARE.SARE_TARIFF_NOTES',UPDATEABLE,PROPERTY_TRUE);  &amp;#10;else&amp;#10;&#x9;SET_ITEM_PROPERTY('SARE.NBT_ATAR_DESC',ENABLED,PROPERTY_FALSE);&#x9;&amp;#10;&#x9;SET_ITEM_PROPERTY('SARE.SARE_TARIFF_NOTES',ENABLED,PROPERTY_FALSE);&#x9;&amp;#10;end if;&#x9;&amp;#10;&amp;#10;--EC2026&amp;#10;IF :SARE.SARE_ID IS NOT NULL AND&amp;#10;&#x9; :SARE.SARE_URN_ID = 'CREATED NOT LOADED SMP0026'&amp;#10;THEN&amp;#10;  DECLARE&amp;#10;  &#x9;   CURSOR C1 (CP_SURNAME VARCHAR2) IS&amp;#10;  &#x9;   SELECT  MBJS_OS_USERNAME, MBJS_JOB_CREATED_DATE&amp;#10;    FROM module_batch_job_submissions&amp;#10;    WHERE instr(MBJS_COMMAND,'@SMR_TOP:SMD0026')&lt;> 0&amp;#10;    and instr(mbjs_command,:SARE.SARE_ST_REG_NO) &lt;> 0&amp;#10;    AND INSTR(MBJS_COMMAND,CP_SURNAME) &lt;> 0&amp;#10;    and instr(mbjs_command,:SARE.SARE_DATETIME) &lt;> 0&amp;#10;    and instr(mbjs_command,'&quot;'||:SARE.SARE_SCORED_ITEMS_CORRECT||'&quot;') &lt;> 0;&amp;#10;    &amp;#10;    &amp;#10;  &#x9; --CURSOR C1 (CP_SURNAME VARCHAR2) IS&amp;#10;  --&#x9; SELECT  MBJS_OS_USERNAME, MBJS_JOB_CREATED_DATE&amp;#10;  --&#x9; FROM module_batch_job_submissi